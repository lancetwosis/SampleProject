        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{Binding RelativeSource={RelativeSource AncestorType={x:Type controls:RadComboBox}}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopBottomLeft}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
