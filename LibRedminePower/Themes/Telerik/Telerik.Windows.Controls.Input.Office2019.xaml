    <Style x:Key="MaskedInputClearButtonStyle"
           TargetType="telerik1:RadButton">
        <Setter Property="Template" Value="{StaticResource MaskedInputClearButtonTemplate}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=IconBrush}" />
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
