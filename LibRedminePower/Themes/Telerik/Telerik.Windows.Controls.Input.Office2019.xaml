    <Style x:Key="RadColorSelectorStyle"
           TargetType="controls:RadColorSelector">
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="MainPaletteHeaderText" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureConverter}, ConverterParameter=ColorSelectorMainPaletteHeaderText}" />
        <Setter Property="NoColorText" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureConverter}, ConverterParameter=ColorSelectorAutomaticColorText}" />
        <Setter Property="RecentColorsHeaderText" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureConverter}, ConverterParameter=ColorSelectorRecentColorsHeaderText}" />
        <Setter Property="StandardPaletteHeaderText" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureConverter}, ConverterParameter=ColorSelectorStandardPaletteHeaderText}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:RadColorSelector">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid x:Name="RootElement">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <telerik1:RadButton x:Name="PaletteHeader"
                                                Grid.Row="0"
                                                Content="{TemplateBinding NoColorText}"
                                                Style="{StaticResource RadColorPicker_HeaderButtonStyle}"
                                                Tag="{Binding AutomaticColor,
                                                              RelativeSource={RelativeSource TemplatedParent},
                                                              Converter={StaticResource converter}}" />
                            <Border Grid.Row="1"
                                    MinHeight="22"
                                    Margin="1,0"
                                    Background="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}">
                                <TextBlock x:Name="mainPaletteHeader"
                                           Margin="3,0"
                                           VerticalAlignment="Center"
                                           FontSize="{TemplateBinding FontSize}"
                                           FontStyle="{TemplateBinding FontStyle}"
                                           FontWeight="{TemplateBinding FontWeight}"
                                           Foreground="{TemplateBinding Foreground}"
                                           Text="{TemplateBinding MainPaletteHeaderText}" />
                            </Border>
                            <controls:RadColorPaletteView x:Name="HeaderPalette"
                                                          Grid.Row="2"
                                                          Padding="3"
                                                          BorderThickness="0"
                                                          ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                                          ItemTemplate="{TemplateBinding PaletteItemsTemplate}">
                                <controls:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <primitives:RadUniformGrid Columns="10" />
                                    </ItemsPanelTemplate>
                                </controls:RadColorPaletteView.ItemsPanel>
                            </controls:RadColorPaletteView>
                            <controls:RadColorPaletteView x:Name="MainPalette"
                                                          Grid.Row="3"
                                                          Padding="3"
                                                          BorderThickness="0"
                                                          ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                                          ItemContainerStyle="{TemplateBinding MainPaletteItemsStyle}"
                                                          ItemTemplate="{TemplateBinding PaletteItemsTemplate}"
                                                          PaletteColumnsCount="{TemplateBinding MainPaletteColumnsCount}" />
                            <Border x:Name="StandardPaletteBorder"
                                    Grid.Row="4"
                                    MinHeight="22"
                                    Margin="1,0"
                                    Background="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}">
                                <TextBlock x:Name="standardPaletteHeader"
                                           Margin="3,0"
                                           VerticalAlignment="Center"
                                           FontSize="{TemplateBinding FontSize}"
                                           FontWeight="{TemplateBinding FontWeight}"
                                           Foreground="{TemplateBinding Foreground}"
                                           Text="{TemplateBinding StandardPaletteHeaderText}" />
                            </Border>
                            <controls:RadColorPaletteView x:Name="StandartPalette"
                                                          Grid.Row="5"
                                                          Padding="3"
                                                          BorderThickness="0"
                                                          ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                                          ItemTemplate="{TemplateBinding PaletteItemsTemplate}">
                                <controls:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <primitives:RadUniformGrid Columns="10" />
                                    </ItemsPanelTemplate>
                                </controls:RadColorPaletteView.ItemsPanel>
                            </controls:RadColorPaletteView>
                            <Border x:Name="RecentColorsPaletteBorder"
                                    Grid.Row="6"
                                    MinHeight="22"
                                    Margin="1,0"
                                    Padding="3"
                                    Background="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}"
                                    Visibility="Collapsed">
                                <TextBlock x:Name="RecentColorsPaletteHeader"
                                           Margin="3,0"
                                           VerticalAlignment="Center"
                                           FontSize="{TemplateBinding FontSize}"
                                           FontWeight="{TemplateBinding FontWeight}"
                                           Foreground="{TemplateBinding Foreground}"
                                           Text="{TemplateBinding RecentColorsHeaderText}" />
                            </Border>
                            <controls:RadColorPaletteView x:Name="RecentColorsPalette"
                                                          Grid.Row="7"
                                                          Padding="3"
                                                          BorderThickness="0"
                                                          ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                                          ItemTemplate="{TemplateBinding PaletteItemsTemplate}"
                                                          Visibility="Collapsed">
                                <controls:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <primitives:RadUniformGrid Columns="10"
                                                                   Rows="1" />
                                    </ItemsPanelTemplate>
                                </controls:RadColorPaletteView.ItemsPanel>
                            </controls:RadColorPaletteView>
                            <Border x:Name="AdditionalContentBorder"
                                    Grid.Row="8"
                                    Background="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}"
                                    Visibility="Collapsed">
                                <ContentPresenter Content="{TemplateBinding AdditionalContent}"
                                                  ContentTemplate="{TemplateBinding AdditionalContentTemplate}" />
                            </Border>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadColorPickerStyle"
           TargetType="controls:RadColorPicker">
        <Setter Property="MainPaletteColumnsCount" Value="10" />
        <Setter Property="MainPaletteOrientation" Value="Vertical" />
        <Setter Property="MainPalette" Value="Office" />
        <Setter Property="HeaderPaletteVisibility" Value="Visible" />
        <Setter Property="MainPaletteHeaderText" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureConverter}, ConverterParameter=ColorSelectorMainPaletteHeaderText}" />
        <Setter Property="NoColorText" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureConverter}, ConverterParameter=ColorSelectorAutomaticColorText}" />
        <Setter Property="RecentColorsHeaderText" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureConverter}, ConverterParameter=ColorSelectorRecentColorsHeaderText}" />
        <Setter Property="StandardPaletteHeaderText" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureConverter}, ConverterParameter=ColorSelectorStandardPaletteHeaderText}" />
        <Setter Property="StandardPaletteVisibility" Value="Visible" />
        <Setter Property="NoColorVisibility" Value="Visible" />
        <Setter Property="SelectedColor" Value="Black" />
        <Setter Property="StandardPalette" Value="Standard" />
        <Setter Property="HeaderPalette" Value="Office" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="VerticalAlignment" Value="Stretch" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Padding" Value="3,2" />
        <Setter Property="ColorSelectorStyle" Value="{StaticResource RadColorSelectorStyle}" />
        <Setter Property="SplitButtonStyle" Value="{StaticResource RadSplitButtonStyle}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=ButtonBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="AutomaticColor" Value="Black" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <StackPanel HorizontalAlignment="Center"
                                VerticalAlignment="Center">
                        <telerik1:RadGlyph FontSize="16"
                                           Foreground="{Binding RelativeSource={RelativeSource AncestorType={x:Type telerik1:RadButton},
                                                                                               Mode=FindAncestor},
                                                                Path=Foreground,
                                                                Mode=OneWay}"
                                           Glyph="{StaticResource GlyphForegroundColor}" />
                        <Rectangle Width="18"
                                   Height="3"
                                   Fill="{Binding Converter={StaticResource converter}}" />
                    </StackPanel>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:RadColorPicker">
                    <Grid x:Name="Root"
                          Width="{TemplateBinding Width}"
                          Height="{TemplateBinding Height}"
                          HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                          VerticalAlignment="{TemplateBinding VerticalAlignment}">
                        <Border x:Name="BorderVisual"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}" />
                        <telerik1:RadSplitButton x:Name="SplitButton"
                                                 Padding="{TemplateBinding Padding}"
                                                 HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                 VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                                 helpers:ThemeHelper.CheckedBrush="{telerik1:Office2019Resource ResourceKey=AccentPressedBackgroundBrush}"
                                                 Background="{TemplateBinding Background}"
                                                 BorderBrush="{TemplateBinding BorderBrush}"
                                                 BorderThickness="{TemplateBinding BorderThickness}"
                                                 Content="{TemplateBinding Content}"
                                                 ContentTemplate="{TemplateBinding ContentTemplate}"
                                                 DropDownPlacement="{Binding Path=DropDownPlacement,
                                                                             RelativeSource={RelativeSource TemplatedParent},
                                                                             Mode=TwoWay}"
                                                 IsChecked="{Binding Path=IsOpen,
                                                                     RelativeSource={RelativeSource Self}}"
                                                 IsOpen="{Binding Path=IsDropDownOpen,
                                                                  Mode=TwoWay,
                                                                  RelativeSource={RelativeSource TemplatedParent}}"
                                                 Style="{TemplateBinding SplitButtonStyle}">
                            <telerik1:RadSplitButton.DropDownContent>
                                <controls:RadColorSelector x:Name="ColorSelector"
                                                           AdditionalContent="{TemplateBinding AdditionalContent}"
                                                           AdditionalContentTemplate="{TemplateBinding AdditionalContentTemplate}"
                                                           AutomaticColor="{Binding AutomaticColor,
                                                                                    Mode=TwoWay,
                                                                                    RelativeSource={RelativeSource TemplatedParent}}"
                                                           ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                                           Command="{TemplateBinding Command}"
                                                           CommandParameter="{TemplateBinding CommandParameter}"
                                                           CommandTarget="{TemplateBinding CommandTarget}"
                                                           FontStyle="{TemplateBinding FontStyle}"
                                                           Foreground="{TemplateBinding Foreground}"
                                                           HeaderPalette="{TemplateBinding HeaderPalette}"
                                                           HeaderPaletteItemsSource="{TemplateBinding HeaderPaletteItemsSource}"
                                                           HeaderPaletteVisibility="{TemplateBinding HeaderPaletteVisibility}"
                                                           IsRecentColorsActive="{TemplateBinding IsRecentColorsActive}"
                                                           MainPalette="{TemplateBinding MainPalette}"
                                                           MainPaletteColumnsCount="{TemplateBinding MainPaletteColumnsCount}"
                                                           MainPaletteHeaderText="{TemplateBinding MainPaletteHeaderText}"
                                                           MainPaletteItemsSource="{TemplateBinding MainPaletteItemsSource}"
                                                           MainPaletteItemsStyle="{TemplateBinding MainPaletteItemsStyle}"
                                                           MainPaletteOrientation="{TemplateBinding MainPaletteOrientation}"
                                                           NoColorText="{TemplateBinding NoColorText}"
                                                           NoColorVisibility="{TemplateBinding NoColorVisibility}"
                                                           PaletteItemsTemplate="{TemplateBinding PaletteItemsTemplate}"
                                                           RecentColorsHeaderText="{TemplateBinding RecentColorsHeaderText}"
                                                           RecentColorsItemsSource="{TemplateBinding RecentColorsItemsSource}"
                                                           RecentColorsVisibility="{TemplateBinding RecentColorsVisibility}"
                                                           SelectedColor="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                   Path=SelectedColor,
                                                                                   Mode=TwoWay}"
                                                           SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                  Path=SelectedItem,
                                                                                  Mode=TwoWay}"
                                                           StandardPalette="{TemplateBinding StandardPalette}"
                                                           StandardPaletteHeaderText="{TemplateBinding StandardPaletteHeaderText}"
                                                           StandardPaletteItemsSource="{TemplateBinding StandardPaletteItemsSource}"
                                                           StandardPaletteVisibility="{TemplateBinding StandardPaletteVisibility}"
                                                           Style="{TemplateBinding ColorSelectorStyle}" />
                            </telerik1:RadSplitButton.DropDownContent>
                        </telerik1:RadSplitButton>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger SourceName="SplitButton" Property="IsChecked" Value="True">
                            <Setter TargetName="SplitButton" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
