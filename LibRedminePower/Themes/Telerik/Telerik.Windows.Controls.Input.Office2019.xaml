                        <ContentPresenter x:Name="Content"
                                          MinWidth="18"
                                          MinHeight="18"
                                          ToolTipService.ToolTip="{Binding}" />
                        <Border x:Name="NormalVisual"
                                MinWidth="{StaticResource ColorPaletteViewItemMinSize}"
                                MinHeight="{StaticResource ColorPaletteViewItemMinSize}"
                                VerticalAlignment="Stretch"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{Binding PositionState,
                                                          RelativeSource={RelativeSource TemplatedParent},
                                                          Converter={StaticResource PositionConverter}}">
                            <Border x:Name="InnerborderContent"
                                    BorderThickness="1" />
                        </Border>
                        <Border x:Name="MouseOverVisual"
                                MinWidth="{StaticResource ColorPaletteViewItemMinSize}"
                                MinHeight="{StaticResource ColorPaletteViewItemMinSize}"
                                VerticalAlignment="Stretch"
                                BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                      Path=(helpers:ThemeHelper.MouseOverBrush)}"
                                BorderThickness="1"
                                Opacity="0">
                            <Border BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                          Path=(helpers:ThemeHelper.MouseOverBrush)}"
                                    BorderThickness="1" />
                        </Border>
                        <Border x:Name="SelectedVisual"
                                MinWidth="{StaticResource ColorPaletteViewItemMinSize}"
                                MinHeight="{StaticResource ColorPaletteViewItemMinSize}"
                                VerticalAlignment="Stretch"
                                BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                      Path=(helpers:ThemeHelper.CheckedBrush)}"
                                BorderThickness="1"
                                Opacity="0">
                            <Border BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                          Path=(helpers:ThemeHelper.CheckedBrush)}"
                                    BorderThickness="1" />
                        </Border>
                        <Border x:Name="FocusVisual"
                                MinWidth="{StaticResource ColorPaletteViewItemMinSize}"
                                MinHeight="{StaticResource ColorPaletteViewItemMinSize}"
                                VerticalAlignment="Stretch"
                                BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                      Path=(helpers:ThemeHelper.FocusBrush)}"
                                BorderThickness="1"
                                Opacity="0"
                                SnapsToDevicePixels="True" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="MouseOverVisual" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="SelectedVisual" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="FocusVisual" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledForegroundBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
