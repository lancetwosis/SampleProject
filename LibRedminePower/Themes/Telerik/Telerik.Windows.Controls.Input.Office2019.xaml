    </Style>
    <Style x:Key="RadMaskedInputBaseStyle"
           TargetType="controls:RadMaskedInputBase">
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.ReadOnlyBrush" Value="{telerik1:Office2019Resource ResourceKey=ReadOnlyBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik1:Office2019Resource ResourceKey=DisabledBorderBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="5,0" />
        <Setter Property="MinHeight" Value="22" />
        <Setter Property="TextBoxStyle" Value="{StaticResource MaskedInputTextBoxStyle}" />
        <Setter Property="ClearButtonStyle" Value="{StaticResource MaskedInputClearButtonStyle}" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="TextMode" Value="MaskedText" />
        <Setter Property="Validation.ErrorTemplate" Value="{StaticResource ValidationTooltipTemplate}" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:RadMaskedInputBase">
                    <Grid>
                        <Border x:Name="BorderVisual"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        <Border x:Name="FocusVisual"
                                Background="{x:Null}"
                                BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                      Path=(helpers:ThemeHelper.FocusBrush)}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                IsHitTestVisible="False"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                Visibility="Collapsed" />
                        <Grid x:Name="GridContainer">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <telerikMaskedInput:PreviewInputTextBox x:Name="EditorSite"
                                                                    Height="{TemplateBinding Height}"
                                                                    Padding="{TemplateBinding Padding}"
                                                                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                                                    AcceptsReturn="{TemplateBinding AcceptsReturn}"
                                                                    AutomationProperties.HelpText="{TemplateBinding AutomationProperties.HelpText}"
                                                                    AutomationProperties.LabeledBy="{TemplateBinding AutomationProperties.LabeledBy}"
                                                                    AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                                                                    Background="{TemplateBinding Background}"
                                                                    CaretBrush="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}"
                                                                    Cursor="{TemplateBinding Cursor}"
                                                                    Focusable="{TemplateBinding Focusable}"
                                                                    FontFamily="{TemplateBinding FontFamily}"
                                                                    FontSize="{TemplateBinding FontSize}"
                                                                    FontWeight="{TemplateBinding FontWeight}"
                                                                    Foreground="{TemplateBinding Foreground}"
                                                                    IsEnabled="{TemplateBinding IsEnabled}"
                                                                    IsReadOnly="{TemplateBinding IsReadOnly}"
                                                                    Opacity="0"
                                                                    Style="{TemplateBinding TextBoxStyle}"
                                                                    TabIndex="{TemplateBinding TabIndex}"
                                                                    TextAlignment="{Binding HorizontalContentAlignment,
                                                                                            Converter={StaticResource conv},
                                                                                            RelativeSource={RelativeSource Mode=TemplatedParent}}" />
                            <Border x:Name="DisplayTextContent"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="0"
                                    IsHitTestVisible="False">
                                <telerikMaskedInput:PreviewInputTextBox x:Name="DisplayText"
                                                                        Height="{TemplateBinding Height}"
                                                                        Padding="{TemplateBinding Padding}"
                                                                        HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                                        VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                                                        AutomationProperties.HelpText="{TemplateBinding AutomationProperties.HelpText}"
                                                                        AutomationProperties.LabeledBy="{TemplateBinding AutomationProperties.LabeledBy}"
                                                                        AutomationProperties.Name="DisplayTextTextBox"
                                                                        Cursor="{TemplateBinding Cursor}"
                                                                        FontFamily="{TemplateBinding FontFamily}"
                                                                        FontSize="{TemplateBinding FontSize}"
                                                                        FontWeight="{TemplateBinding FontWeight}"
                                                                        Foreground="{TemplateBinding Foreground}"
                                                                        IsEnabled="{TemplateBinding IsEnabled}"
                                                                        IsReadOnly="{TemplateBinding IsReadOnly}"
                                                                        IsTabStop="False"
                                                                        Style="{TemplateBinding TextBoxStyle}"
                                                                        Text="{Binding Path=Text,
                                                                                       RelativeSource={RelativeSource Mode=TemplatedParent},
                                                                                       Mode=OneWay}"
                                                                        TextAlignment="{Binding HorizontalContentAlignment,
                                                                                                Converter={StaticResource conv},
                                                                                                RelativeSource={RelativeSource Mode=TemplatedParent}}" />
                            </Border>
                            <Border x:Name="EmptyContent"
                                    Padding="2,0,0,0"
                                    IsHitTestVisible="False"
                                    Visibility="Collapsed">
                                <ContentControl x:Name="Content"
                                                Margin="{TemplateBinding Padding}"
                                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                Content="{TemplateBinding EmptyContent}"
                                                ContentTemplate="{TemplateBinding EmptyContentTemplate}"
                                                FontStyle="Italic"
                                                Foreground="{TemplateBinding Foreground}"
                                                IsTabStop="False"
                                                Opacity="0.8" />
                            </Border>
                            <telerik1:RadButton x:Name="ClearButton"
                                                Grid.Column="1"
                                                Command="{TemplateBinding ClearCommand}"
                                                Opacity="0"
                                                Style="{TemplateBinding ClearButtonStyle}"
                                                Visibility="{Binding Path=IsClearButtonVisible,
                                                                     RelativeSource={RelativeSource Mode=TemplatedParent},
                                                                     Converter={StaticResource BooleanToVisibilityConverter}}" />
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsReadOnly" Value="True">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=ReadOnlyOpacity}" />
                            <Setter TargetName="DisplayText" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=ReadOnlyOpacity}" />
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2019Resource ResourceKey=ReadOnlyBackgroundBrush}" />
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.ReadOnlyBrush)}" />
                        </Trigger>
                        <Trigger Property="IsKeyboardFocusWithin" Value="True">
                            <Setter TargetName="DisplayTextContent" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="EditorSite" Property="Opacity" Value="1" />
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Content" Property="Opacity" Value="1" />
                            <Setter TargetName="ClearButton" Property="Opacity" Value="1" />
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}" />
                        </Trigger>

                        <!--  Content State - Empty  -->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsKeyboardFocusWithin" Value="False" />
                                <Condition Property="IsEmpty" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="EmptyContent" Property="Visibility" Value="Visible" />
                            <Setter TargetName="DisplayTextContent" Property="Visibility" Value="Collapsed" />
                        </MultiTrigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="DisplayText" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}" />
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledForegroundBrush}" />
                        </Trigger>

                        <!--  Invalid  -->
                        <Trigger Property="Validation.HasError" Value="True">
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="telerik1:ValidationErrorTemplateHelper.ShowWhenFocused" Value="True">
                <Setter Property="Validation.ErrorTemplate" Value="{StaticResource StaticValidationOnFocusTooltipTemplate}" />
            </Trigger>
        </Style.Triggers>
    </Style>
