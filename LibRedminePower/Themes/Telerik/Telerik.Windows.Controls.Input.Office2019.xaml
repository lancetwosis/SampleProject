    <ControlTemplate x:Key="RadCalculatorTemplate"
                     TargetType="controls:RadCalculator">
        <Grid Background="Transparent"
              SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
            <Border Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
            <Grid x:Name="Root"
                  Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid MinHeight="85">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <TextBox x:Name="DisplayTextBox"
                             Grid.Row="1"
                             Grid.ColumnSpan="2"
                             Margin="10,0"
                             AutomationProperties.Name="DisplayTextBox"
                             Foreground="{TemplateBinding Foreground}"
                             Style="{StaticResource CalculatorDisplayValue_Style}"
                             Text="{Binding DisplayedValue,
                                            RelativeSource={RelativeSource TemplatedParent}}" />
                    <Canvas x:Name="MemoryIndicatorCanvas">
                        <TextBlock x:Name="MemoryIndicatorTextBox"
                                   Margin="10,5,0,0"
                                   VerticalAlignment="Top"
                                   Text="M"
                                   Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                        Path=IsMemoryUpdated,
                                                        Converter={StaticResource BooleanToVisibilityConverter}}" />
                    </Canvas>
                    <TextBox x:Name="HistoryTextBox"
                             Grid.Column="1"
                             Foreground="{TemplateBinding Foreground}"
                             Style="{StaticResource CalculatorHistoryDisplay_Style}"
                             Text="{Binding RelativeSource={RelativeSource TemplatedParent},
                                            Path=History,
                                            Mode=OneWay}" />
                </Grid>
                <Grid x:Name="btnContainer"
                      Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition x:Name="row" />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <telerik:RadButton x:Name="Mc_Button"
                                       Grid.Column="0"
                                       Command="{x:Static controls:RadCalculatorCommands.MemoryClear}"
                                       Content="MC"
                                       Style="{StaticResource CalculatorMemoryButtonStyle}" />
                    <telerik:RadButton x:Name="Mr_Button"
                                       Grid.Column="1"
                                       Command="{x:Static controls:RadCalculatorCommands.MemoryRecall}"
                                       Content="MR"
                                       Style="{StaticResource CalculatorMemoryButtonStyle}" />
                    <telerik:RadButton x:Name="Ms_Button"
                                       Grid.Column="2"
                                       Command="{x:Static controls:RadCalculatorCommands.MemoryStore}"
                                       Content="MS"
                                       Style="{StaticResource CalculatorMemoryButtonStyle}" />
                    <telerik:RadButton x:Name="Mplus_Button"
                                       Grid.Column="3"
                                       Command="{x:Static controls:RadCalculatorCommands.MemoryPlus}"
                                       Content="M+"
                                       Style="{StaticResource CalculatorMemoryButtonStyle}" />
                    <telerik:RadButton x:Name="Mminus_Button"
                                       Grid.Column="4"
                                       Command="{x:Static controls:RadCalculatorCommands.MemoryMinus}"
                                       Content="M-"
                                       Style="{StaticResource CalculatorMemoryButtonStyle}" />
                    <telerik:RadButton x:Name="Delete_Button"
                                       Grid.Row="1"
                                       Grid.Column="0"
                                       Command="{x:Static controls:RadCalculatorCommands.Delete}"
                                       Content="&lt;"
                                       Style="{StaticResource CalculatorAlternateButtonStyle}" />
                    <telerik:RadButton x:Name="ClearInput_Button"
                                       Grid.Row="1"
                                       Grid.Column="1"
                                       Command="{x:Static controls:RadCalculatorCommands.ClearInput}"
                                       Content="CE"
                                       Style="{StaticResource CalculatorAlternateButtonStyle}" />
                    <telerik:RadButton x:Name="ClearEverything_Button"
                                       Grid.Row="1"
                                       Grid.Column="2"
                                       Command="{x:Static controls:RadCalculatorCommands.ClearEverything}"
                                       Content="C"
                                       Style="{StaticResource CalculatorAlternateButtonStyle}" />
                    <telerik:RadButton x:Name="Negate_Button"
                                       Grid.Row="1"
                                       Grid.Column="3"
                                       Command="{x:Static controls:RadCalculatorCommands.ExecuteUnaryOperation}"
                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},
                                                                  Path=Operations.Negate}"
                                       Content="±"
                                       Style="{StaticResource CalculatorAlternateButtonStyle}" />
                    <telerik:RadButton x:Name="SquareRoot_Button"
                                       Grid.Row="1"
                                       Grid.Column="4"
                                       Command="{x:Static controls:RadCalculatorCommands.ExecuteUnaryOperation}"
                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},
                                                                  Path=Operations.SquareRoot}"
                                       Content="√"
                                       Style="{StaticResource CalculatorAlternateButtonStyle}" />
                    <telerik:RadButton x:Name="input7_Button"
                                       Grid.Row="2"
                                       Grid.Column="0"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="7"
                                       Content="7" />
                    <telerik:RadButton x:Name="input8_Button"
                                       Grid.Row="2"
                                       Grid.Column="1"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="8"
                                       Content="8" />
                    <telerik:RadButton x:Name="input9_Button"
                                       Grid.Row="2"
                                       Grid.Column="2"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="9"
                                       Content="9" />
                    <telerik:RadButton x:Name="Divide_Button"
                                       Grid.Row="2"
                                       Grid.Column="3"
                                       Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},
                                                                  Path=Operations.Divide}"
                                       Content="/" />
                    <telerik:RadButton x:Name="Percent_Button"
                                       Grid.Row="2"
                                       Grid.Column="4"
                                       Command="{x:Static controls:RadCalculatorCommands.ExecutePercentOperation}"
                                       Content="%"
                                       Style="{StaticResource CalculatorAlternateButtonStyle}" />
                    <telerik:RadButton x:Name="input4_Button"
                                       Grid.Row="3"
                                       Grid.Column="0"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="4"
                                       Content="4" />
                    <telerik:RadButton x:Name="input5_Button"
                                       Grid.Row="3"
                                       Grid.Column="1"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="5"
                                       Content="5" />
                    <telerik:RadButton x:Name="input6_Button"
                                       Grid.Row="3"
                                       Grid.Column="2"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="6"
                                       Content="6" />
                    <telerik:RadButton x:Name="Multiply_Button"
                                       Grid.Row="3"
                                       Grid.Column="3"
                                       Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},
                                                                  Path=Operations.Multiply}"
                                       Content="*" />
                    <telerik:RadButton x:Name="Reciprocal_Button"
                                       Grid.Row="3"
                                       Grid.Column="4"
                                       Command="{x:Static controls:RadCalculatorCommands.ExecuteUnaryOperation}"
                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},
                                                                  Path=Operations.Reciprocal}"
                                       Content="1/x"
                                       Style="{StaticResource CalculatorAlternateButtonStyle}" />
                    <telerik:RadButton x:Name="input1_Button"
                                       Grid.Row="4"
                                       Grid.Column="0"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="1"
                                       Content="1" />
                    <telerik:RadButton x:Name="input2_Button"
                                       Grid.Row="4"
                                       Grid.Column="1"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="2"
                                       Content="2" />
                    <telerik:RadButton x:Name="input3_Button"
                                       Grid.Row="4"
                                       Grid.Column="2"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="3"
                                       Content="3" />
                    <telerik:RadButton x:Name="Substract_Button"
                                       Grid.Row="4"
                                       Grid.Column="3"
                                       Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},
                                                                  Path=Operations.Substract}"
                                       Content="-" />
                    <telerik:RadButton x:Name="input0_Button"
                                       Grid.Row="5"
                                       Grid.Column="0"
                                       Grid.ColumnSpan="2"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="0"
                                       Content="0" />
                    <telerik:RadButton x:Name="FloatingPoint_Button"
                                       Grid.Row="5"
                                       Grid.Column="2"
                                       Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},
                                                                  Path=DecimalSeparator}"
                                       Content="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},
                                                         Path=DecimalSeparator}" />
                    <telerik:RadButton x:Name="Add_Button"
                                       Grid.Row="5"
                                       Grid.Column="3"
                                       Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent},
                                                                  Path=Operations.Add}"
                                       Content="+" />
                    <telerik:RadButton x:Name="Equals_Button"
                                       Grid.Row="4"
                                       Grid.RowSpan="2"
                                       Grid.Column="4"
                                       Command="{x:Static controls:RadCalculatorCommands.ExecuteBinaryOperation}"
                                       Content="="
                                       Style="{StaticResource CalculatorAlternateButtonStyle}" />
                </Grid>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="MemoryButtonsVisibility" Value="Collapsed">
                <Setter TargetName="Mc_Button" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="Mr_Button" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="Ms_Button" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="Mplus_Button" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="Mminus_Button" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="row" Property="Height" Value="0" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="MemoryIndicatorTextBox" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                <Setter TargetName="HistoryTextBox" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledForegroundBrush}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadCalculatorStyle"
           TargetType="controls:RadCalculator">
        <Setter Property="Template" Value="{StaticResource RadCalculatorTemplate}" />
        <Setter Property="MinHeight" Value="294" />
        <Setter Property="MinWidth" Value="175" />
        <Setter Property="Padding" Value="5,5,4,4" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Style.Resources>
            <Style BasedOn="{StaticResource CalculatorButtonStyle}"
                   TargetType="telerik:RadButton" />
            <Style x:Key="CalculatorAlternateButtonStyle"
                   BasedOn="{StaticResource CalculatorAlternateButtonStyle}"
                   TargetType="telerik:RadButton" />
            <Style x:Key="CalculatorMemoryButtonStyle"
                   BasedOn="{StaticResource CalculatorMemoryButtonStyle}"
                   TargetType="telerik:RadButton" />
        </Style.Resources>
    </Style>
    <Style BasedOn="{StaticResource RadCalculatorStyle}"
           TargetType="controls:RadCalculator" />
    <Style x:Key="LabelStyle"
           TargetType="telerik1:Label">
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Padding" Value="0,3" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik1:Label">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <ContentPresenter x:Name="Content"
                                              Margin="{TemplateBinding Padding}"
                                              HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                              VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                              RecognizesAccessKey="True" />
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledForegroundBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style BasedOn="{StaticResource LabelStyle}"
           TargetType="telerik1:Label" />
    <Style x:Key="WatermarkTextBoxLabelStyle"
           BasedOn="{StaticResource LabelStyle}"
           TargetType="telerik1:Label">
        <Setter Property="IsHitTestVisible" Value="False" />
        <Setter Property="Opacity" Value="0.8" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Padding" Value="7,0" />
        <Setter Property="Margin" Value="0,5" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Style.Triggers>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RelativeSource={RelativeSource AncestorType=telerik1:RadWatermarkTextBox}, Path=HasLabel}" Value="True" />
                    <Condition Binding="{Binding RelativeSource={RelativeSource AncestorType=telerik1:RadWatermarkTextBox}, Path=IsLabelFloated}" Value="True" />
                </MultiDataTrigger.Conditions>
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=AccentBackgroundBrush}" />
                <Setter Property="Opacity" Value="1" />
            </MultiDataTrigger>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerik1:RadWatermarkTextBox},
                                           Path=HasLabel}"
                         Value="False">
                <Setter Property="Margin" Value="0" />
            </DataTrigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" Value="1" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="RadWatermarkTextBoxStyle"
           TargetType="telerik1:RadWatermarkTextBox">
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.ReadOnlyBrush" Value="{telerik1:Office2019Resource ResourceKey=ReadOnlyBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik1:Office2019Resource ResourceKey=DisabledBorderBrush}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="ReadOnlyBackground" Value="{telerik1:Office2019Resource ResourceKey=ReadOnlyBackgroundBrush}" />
        <Setter Property="DisabledBackground" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="5,0" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="AllowDrop" Value="True" />
        <Setter Property="IsTabStop" Value="True" />
        <Setter Property="KeyboardNavigation.TabNavigation" Value="Once" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="telerik1:CaretBrushHelper.CaretBrush" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="SelectionBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentBackgroundBrush}" />
        <Setter Property="SelectionOpacity" Value="0.35" />
        <Setter Property="Validation.ErrorTemplate" Value="{StaticResource ValidationTooltipTemplate}" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="MinHeight" Value="22" />
        <Setter Property="LabelStyle" Value="{StaticResource WatermarkTextBoxLabelStyle}" />
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:AnimationGroup AnimationName="LabelFloatAnimation">
                        <animation:LabelPositionAnimation Direction="Out"
                                                          TargetElementName="PART_LabelVisualElement"
                                                          Duration="00:00:00.175" />
                        <animation:FontSizeAnimation Direction="Out"
                                                     TargetElementName="PART_LabelVisualElement"
                                                     Duration="00:00:00.175" />
                    </animation:AnimationGroup>
                    <animation:AnimationGroup AnimationName="LabelCenterAnimation">
                        <animation:LabelPositionAnimation Direction="In"
                                                          TargetElementName="PART_LabelVisualElement"
                                                          Duration="00:00:00.175" />
                        <animation:FontSizeAnimation Direction="In"
                                                     TargetElementName="PART_LabelVisualElement"
                                                     Duration="00:00:00.175" />
                    </animation:AnimationGroup>
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik1:RadWatermarkTextBox">
                    <Grid x:Name="RootElement">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="PART_LabelRowDefinition" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Border x:Name="BorderVisual"
                                Grid.RowSpan="2"
                                Grid.ColumnSpan="2"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}" />
                        <ScrollViewer x:Name="PART_ContentHost"
                                      Grid.RowSpan="2"
                                      Margin="{TemplateBinding BorderThickness}"
                                      Padding="{TemplateBinding Padding}"
                                      VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                      Background="Transparent"
                                      BorderThickness="0"
                                      IsTabStop="False"
                                      SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        <ContentControl x:Name="WatermarkVisualElement"
                                        Grid.RowSpan="2"
                                        Margin="1,0,0,0"
                                        Padding="{TemplateBinding Padding}"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        BorderThickness="{TemplateBinding BorderThickness}"
                                        Content="{TemplateBinding WatermarkContent}"
                                        ContentTemplate="{TemplateBinding WatermarkTemplate}"
                                        FontStyle="Italic"
                                        Foreground="{TemplateBinding Foreground}"
                                        IsHitTestVisible="False"
                                        IsTabStop="False"
                                        Opacity="0.8"
                                        Visibility="Hidden">
                            <ContentControl.Template>
                                <ControlTemplate TargetType="ContentControl">
                                    <Border BorderBrush="{x:Null}"
                                            BorderThickness="{TemplateBinding BorderThickness}">
                                        <ContentPresenter Margin="{TemplateBinding Padding}" />
                                    </Border>
                                </ControlTemplate>
                            </ContentControl.Template>
                        </ContentControl>
                        <ContentPresenter x:Name="AdditionalContentHost"
                                          Grid.RowSpan="2"
                                          Grid.Column="1"
                                          Content="{TemplateBinding AdditionalContent}"
                                          ContentTemplate="{TemplateBinding AdditionalContentTemplate}" />
                        <telerik1:Label x:Name="PART_LabelVisualElement"
                                        Grid.RowSpan="2"
                                        Content="{TemplateBinding Label}"
                                        Style="{TemplateBinding LabelStyle}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}" />
                        </Trigger>
                        <Trigger Property="IsReadOnly" Value="True">
                            <Setter TargetName="WatermarkVisualElement" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=ReadOnlyOpacity}" />
                            <Setter TargetName="PART_ContentHost" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=ReadOnlyOpacity}" />
                            <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ReadOnlyBackground}" />
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.ReadOnlyBrush)}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="PART_ContentHost" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="WatermarkVisualElement" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DisabledBackground}" />
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}" />
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledForegroundBrush}" />
                        </Trigger>
                        <Trigger Property="IsWatermarkVisible" Value="True">
                            <Setter TargetName="WatermarkVisualElement" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsKeyboardFocused" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBrush)}" />
                        </Trigger>
                        <Trigger Property="WatermarkBehavior" Value="HideOnTextEntered">
                            <Setter TargetName="WatermarkVisualElement" Property="Margin" Value="2,0,0,0" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsReadOnly" Value="True" />
                                <Condition Property="IsWatermarkVisible" Value="False" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="WatermarkVisualElement" Property="Visibility" Value="Hidden" />
                        </MultiTrigger>
                        <Trigger Property="HasLabel" Value="True">
                            <Setter TargetName="PART_ContentHost" Property="Grid.Row" Value="1" />
                            <Setter TargetName="PART_ContentHost" Property="Grid.RowSpan" Value="1" />
                            <Setter TargetName="WatermarkVisualElement" Property="Grid.Row" Value="1" />
