                               CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                      Path=(helpers:ThemeHelper.CornerRadius),
                                                      Converter={StaticResource CornerRadiusConverter},
                                                      ConverterParameter=TopBottomRight}"
                               KeyboardNavigation.IsTabStop="False"
                               Style="{StaticResource AutoSuggestBoxQueryButtonStyle}"
                               Visibility="{TemplateBinding QueryButtonVisibility}" />
            <telerik:RadWatermarkTextBox x:Name="PART_TextBox"
                                         Grid.Column="0"
                                         MinHeight="0"
                                         Margin="{TemplateBinding BorderThickness}"
                                         Padding="{TemplateBinding Padding}"
                                         VerticalAlignment="Stretch"
                                         HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                         VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                         AutomationProperties.HelpText="{TemplateBinding AutomationProperties.HelpText}"
                                         AutomationProperties.LabeledBy="{TemplateBinding AutomationProperties.LabeledBy}"
                                         AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                                         Background="{x:Null}"
                                         BorderBrush="{x:Null}"
                                         BorderThickness="0"
                                         DisabledBackground="{x:Null}"
                                         Focusable="{TemplateBinding Focusable}"
                                         FontFamily="{TemplateBinding FontFamily}"
                                         FontSize="{TemplateBinding FontSize}"
                                         Foreground="{TemplateBinding Foreground}"
                                         ReadOnlyBackground="{x:Null}"
                                         WatermarkBehavior="{TemplateBinding WatermarkBehavior}"
                                         WatermarkContent="{TemplateBinding WatermarkContent}"
                                         WatermarkTemplate="{TemplateBinding WatermarkTemplate}" />
            <Border x:Name="FocusVisual"
                    Grid.ColumnSpan="3"
                    Background="{x:Null}"
                    BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent},
                                          Path=(helpers:ThemeHelper.FocusBrush)}"
                    BorderThickness="1"
                    IsHitTestVisible="False"
                    Opacity="0"
                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
            <Popup x:Name="PART_Popup"
                   Focusable="{TemplateBinding Focusable}">
                <Grid x:Name="PopupRoot"
                      Width="{TemplateBinding ActualWidth}"
                      MinWidth="{TemplateBinding MinDropDownWidth}"
                      MaxWidth="{TemplateBinding MaxDropDownWidth}"
                      MaxHeight="{TemplateBinding MaxDropDownHeight}">
                    <telerik:RadListBox x:Name="PART_ItemsHost"
                                        DisplayMemberPath="{TemplateBinding DisplayMemberPath}"
                                        EnableSelectionCaching="False"
                                        GroupStyleSelector="{TemplateBinding GroupStyleSelector}"
                                        IsSynchronizedWithCurrentItem="False"
                                        ItemContainerStyle="{TemplateBinding ItemContainerStyle}"
                                        ItemContainerStyleSelector="{TemplateBinding ItemContainerStyleSelector}"
                                        ItemTemplate="{TemplateBinding ItemTemplate}"
                                        ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}"
                                        ItemsPanel="{TemplateBinding ItemsPanel}"
                                        ItemsSource="{TemplateBinding ItemsSource}"
                                        ScrollViewer.CanContentScroll="{TemplateBinding ScrollViewer.CanContentScroll}"
                                        SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                    <Border Background="{Binding Background,
                                                 ElementName=PART_ItemsHost}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="1"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                            Visibility="{Binding ElementName=PART_NoResultsPresenter,
                                                 Path=Visibility}">
                        <ContentPresenter x:Name="PART_NoResultsPresenter"
                                          Margin="5,2"
                                          ContentSource="NoResultsContent"
                                          Visibility="Collapsed">
                            <ContentPresenter.Resources>
                                <Style TargetType="TextBlock">
                                    <Setter Property="TextTrimming" Value="CharacterEllipsis" />
                                </Style>
                            </ContentPresenter.Resources>
                        </ContentPresenter>
                    </Border>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger SourceName="PART_ItemsHost" Property="Visibility" Value="Collapsed">
                <Setter TargetName="PART_NoResultsPresenter" Property="Visibility" Value="Visible" />
            </Trigger>

            <!--  MouseOver  -->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik:Office2019Resource ResourceKey=AccentMouseOverBorderBrush}" />
            </Trigger>

            <!--  Disabled  -->
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}" />
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledForegroundBrush}" />
            </Trigger>

            <!--  Focused  -->
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter TargetName="FocusVisual" Property="Opacity" Value="1" />
            </Trigger>

            <!--  Padding  -->
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=QueryButtonVisibility}" Value="Collapsed" />
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=ClearButtonVisibility}" Value="Collapsed" />
                </MultiDataTrigger.Conditions>
                <Setter Property="Padding" Value="4,2" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=QueryButtonVisibility}" Value="Collapsed" />
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=ClearButtonVisibility}" Value="Auto" />
                </MultiDataTrigger.Conditions>
                <Setter Property="Padding" Value="4,2" />
            </MultiDataTrigger>
            <Trigger SourceName="PART_QueryButton" Property="IsMouseOver" Value="True">
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}" />
                <Setter TargetName="PART_QueryButton" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}" />
            </Trigger>
            <Trigger SourceName="PART_QueryButton" Property="IsPressed" Value="True">
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBrush)}" />
                <Setter TargetName="PART_QueryButton" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBrush)}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <DataTemplate x:Key="AutoSuggestBoxButtonsContentTemplate">
        <telerik:RadGlyph FontSize="16"
                          Glyph="{Binding}" />
    </DataTemplate>
    <DataTemplate x:Key="AutoSuggestBoxWatermarkTemplate">
        <ContentControl Margin="1,0"
                        Content="{Binding}"
                        IsTabStop="False" />
    </DataTemplate>
    <Style x:Key="RadAutoSuggestBoxStyle"
           TargetType="controls:RadAutoSuggestBox">
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.PressedBrush" Value="{telerik:Office2019Resource ResourceKey=AccentBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik:Office2019Resource ResourceKey=AccentMouseOverBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{telerik:Office2019Resource ResourceKey=AccentBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik:Office2019Resource ResourceKey=DisabledBorderBrush}" />
        <Setter Property="Padding" Value="4,2,0,2" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="ScrollViewer.CanContentScroll" Value="True" />
        <Setter Property="QueryButtonContent" Value="{StaticResource GlyphZoom}" />
        <Setter Property="ClearButtonContent" Value="{StaticResource GlyphClose}" />
        <Setter Property="Template" Value="{StaticResource RadAutoSuggestBoxTemplate}" />
        <Setter Property="Validation.ErrorTemplate" Value="{StaticResource ValidationTooltipTemplate}" />
        <Setter Property="KeyboardNavigation.TabNavigation" Value="Local" />
        <Setter Property="ClearButtonContentTemplate" Value="{StaticResource AutoSuggestBoxButtonsContentTemplate}" />
        <Setter Property="QueryButtonContentTemplate" Value="{StaticResource AutoSuggestBoxButtonsContentTemplate}" />
        <Setter Property="ClearButtonCommand" Value="{x:Static telerik:RadWatermarkTextBoxCommands.Clear}" />
        <Setter Property="QueryButtonCommand" Value="{x:Static telerik:RadAutoSuggestBoxCommands.SubmitQuery}" />
        <Setter Property="WatermarkTemplate" Value="{StaticResource AutoSuggestBoxWatermarkTemplate}" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <VirtualizingStackPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="telerik:ValidationErrorTemplateHelper.ShowWhenFocused" Value="True">
                <Setter Property="Validation.ErrorTemplate" Value="{StaticResource StaticValidationOnFocusTooltipTemplate}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style BasedOn="{StaticResource RadAutoSuggestBoxStyle}"
           TargetType="controls:RadAutoSuggestBox" />

    <!--  RedminePowerカスタマイズ箇所↓↓  -->
    <!--  TextBlock に Disable 時のスタイルが適用されなかったため追加  -->
    <Style x:Key="MyTextBlockStyle"
           BasedOn="{StaticResource {x:Type TextBlock}}"
           TargetType="TextBlock">
        <Setter Property="VerticalAlignment" Value="Center" />
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <!--  RedminePowerカスタマイズ箇所↑↑  -->

</ResourceDictionary>
