                                          HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
                                          VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}">
                                <ItemsPresenter />
                            </ScrollViewer>
                        </Grid>
                    </Border>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>

            <!--  IsKeyboardFocusWithin  -->
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsKeyboardFocusWithin" Value="True" />
                    <Condition Property="IsDropDownOpen" Value="False" />
                    <Condition Property="IsMouseOver" Value="False" />
                </MultiTrigger.Conditions>
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="BackgroundVisual" Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBorderBrush}" />
            </Trigger>
            <Trigger SourceName="PART_DropDownButton" Property="IsPressed" Value="True">
                <Setter TargetName="BackgroundVisual" Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}" />
            </Trigger>
            <Trigger SourceName="PART_DropDownButton" Property="IsChecked" Value="True">
                <Setter TargetName="BackgroundVisual" Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}" />
            </Trigger>
            <Trigger Property="IsReadOnly" Value="True">
                <Setter TargetName="BackgroundVisual" Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=ReadOnlyBorderBrush}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="BackgroundVisual" Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=DisabledBorderBrush}" />
            </Trigger>
            <Trigger Property="ClearSelectionButtonVisibility" Value="Visible">
                <Setter TargetName="PART_ScrollViewer" Property="Padding" Value="0,0,0,5" />
            </Trigger>
        </ControlTemplate.Triggers>
