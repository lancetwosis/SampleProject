                                <!--  対応内容：#165 Window枠に影をつける  -->
                                <Border x:Name="Header"
                                        Background="{telerik1:Office2019Resource ResourceKey=HeaderBackgroundBrush}">
                                    <!--  RedminePowerカスタマイズ箇所↑↑  -->
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <ContentPresenter x:Name="IconContent"
                                                              Grid.Column="0"
                                                              VerticalAlignment="Center"
                                                              Content="{TemplateBinding Icon}"
                                                              ContentTemplate="{TemplateBinding IconTemplate}" />
                                            <ContentPresenter x:Name="HeaderContent"
                                                              Grid.Column="1"
                                                              Margin="10,0"
                                                              VerticalAlignment="Center"
                                                              Content="{TemplateBinding Header}"
                                                              ContentTemplate="{TemplateBinding HeaderTemplate}"
                                                              ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                                              SnapsToDevicePixels="True"
                                                              TextBlock.FontFamily="{TemplateBinding FontFamily}"
                                                              TextBlock.FontSize="{TemplateBinding FontSize}"
                                                              TextBlock.Foreground="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}">
                                                <ContentPresenter.Resources>
                                                    <Style TargetType="TextBlock">
                                                        <Setter Property="TextTrimming" Value="CharacterEllipsis" />
                                                    </Style>
                                                </ContentPresenter.Resources>
                                            </ContentPresenter>
                                        </Grid>
                                        <Border x:Name="PART_HeaderButtonsBorder"
                                                Grid.Column="1">
                                            <!--  RedminePowerカスタマイズ箇所↓↓  -->
                                            <!--  対応内容：#151 Windowボタンの外観修正(最小化・最大化・閉じる)  -->
                                            <Grid x:Name="HeaderButtons"
                                                  Margin="0">
                                                <!--  RedminePowerカスタマイズ箇所↑↑  -->
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition />
                                                    <ColumnDefinition />
                                                    <ColumnDefinition />
                                                    <ColumnDefinition />
                                                </Grid.ColumnDefinitions>
                                                <telerik:RadButton x:Name="PART_MinimizeButton"
                                                                   Grid.Column="0"
                                                                   Command="{x:Static telerik:WindowCommands.Minimize}"
                                                                   CornerRadius="{Binding Path=CornerRadius.TopRight,
                                                                                          RelativeSource={RelativeSource TemplatedParent}}"
                                                                   Style="{StaticResource WindowMinimizeButtonStyle}"
                                                                   Visibility="{Binding IsEnabled,
                                                                                        RelativeSource={RelativeSource Self},
                                                                                        Converter={StaticResource BooleanToVisibilityConverter}}">
                                                    <ToolTipService.ToolTip>
                                                        <ToolTip telerik:LocalizationManager.ResourceKey="Minimize" />
                                                    </ToolTipService.ToolTip>
                                                    <telerik:RadGlyph HorizontalAlignment="Center"
                                                                      VerticalAlignment="Center"
                                                                      FontSize="16"
                                                                      Glyph="{StaticResource GlyphMinimize}" />
                                                </telerik:RadButton>
                                                <telerik:RadButton x:Name="PART_RestoreButton"
                                                                   Grid.Column="1"
                                                                   Command="{x:Static telerik:WindowCommands.Restore}"
                                                                   CornerRadius="{Binding Path=CornerRadius.TopRight,
                                                                                          RelativeSource={RelativeSource TemplatedParent}}"
                                                                   Style="{StaticResource WindowMaximizeButtonStyle}"
                                                                   Visibility="{Binding IsEnabled,
                                                                                        RelativeSource={RelativeSource Self},
                                                                                        Converter={StaticResource BooleanToVisibilityConverter}}">
                                                    <ToolTipService.ToolTip>
                                                        <ToolTip telerik:LocalizationManager.ResourceKey="Restore" />
                                                    </ToolTipService.ToolTip>
                                                    <!--  RedminePowerカスタマイズ箇所↓↓  -->
                                                    <!--  対応内容：#151 Windowボタンの外観修正(最小化・最大化・閉じる)  -->
                                                    <telerik:RadGlyph Margin="0,2,0,0"
                                                                      HorizontalAlignment="Center"
                                                                      VerticalAlignment="Center"
                                                                      FontSize="16"
                                                                      Glyph="{StaticResource GlyphWindowCollapse}" />
                                                    <!--  RedminePowerカスタマイズ箇所↑↑  -->
                                                </telerik:RadButton>
                                                <telerik:RadButton x:Name="PART_MaximizeButton"
                                                                   Grid.Column="2"
                                                                   Command="{x:Static telerik:WindowCommands.Maximize}"
                                                                   CornerRadius="{Binding Path=CornerRadius.TopRight,
                                                                                          RelativeSource={RelativeSource TemplatedParent}}"
                                                                   Style="{StaticResource WindowMaximizeButtonStyle}"
                                                                   Visibility="{Binding IsEnabled,
                                                                                        RelativeSource={RelativeSource Self},
                                                                                        Converter={StaticResource BooleanToVisibilityConverter}}">
                                                    <ToolTipService.ToolTip>
                                                        <ToolTip telerik:LocalizationManager.ResourceKey="Maximize" />
                                                    </ToolTipService.ToolTip>
                                                    <!--  RedminePowerカスタマイズ箇所↓↓  -->
                                                    <!--  対応内容：#151 Windowボタンの外観修正(最小化・最大化・閉じる)  -->
                                                    <telerik:RadGlyph Margin="0,2,0,0"
                                                                      HorizontalAlignment="Center"
                                                                      VerticalAlignment="Center"
                                                                      FontSize="16"
                                                                      Glyph="{StaticResource GlyphWindow}" />
                                                    <!--  RedminePowerカスタマイズ箇所↑↑  -->
                                                </telerik:RadButton>
                                                <telerik:RadButton x:Name="PART_CloseButton"
                                                                   Grid.Column="3"
                                                                   Command="{x:Static telerik:WindowCommands.Close}"
                                                                   CornerRadius="{Binding Path=CornerRadius.TopRight,
                                                                                          RelativeSource={RelativeSource TemplatedParent}}"
                                                                   IsCancel="True"
                                                                   Style="{StaticResource WindowCloseButtonStyle}"
                                                                   Visibility="{Binding IsEnabled,
                                                                                        RelativeSource={RelativeSource Self},
                                                                                        Converter={StaticResource BooleanToVisibilityConverter}}">
                                                    <ToolTipService.ToolTip>
                                                        <ToolTip telerik:LocalizationManager.ResourceKey="Close" />
                                                    </ToolTipService.ToolTip>
                                                    <!--  RedminePowerカスタマイズ箇所↓↓  -->
                                                    <!--  対応内容：#151 Windowボタンの外観修正(最小化・最大化・閉じる)  -->
                                                    <telerik:RadGlyph Margin="0,2,0,0"
                                                                      HorizontalAlignment="Center"
                                                                      VerticalAlignment="Center"
                                                                      FontSize="16"
                                                                      Glyph="{StaticResource GlyphClose}" />
                                                    <!--  RedminePowerカスタマイズ箇所↑↑  -->
                                                </telerik:RadButton>
                                            </Grid>
                                        </Border>
                                    </Grid>
                                </Border>
                                <Border x:Name="ContentOuterBorder"
                                        Grid.Row="1"
                                        Background="{TemplateBinding Background}"
                                        BorderBrush="{TemplateBinding BorderBrush}"
                                        BorderThickness="{Binding BorderThickness,
                                                                  RelativeSource={RelativeSource TemplatedParent},
                                                                  Converter={StaticResource ThicknessToOrientedThicknessConverter},
                                                                  ConverterParameter=BottomLeftRight}"
                                        CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                               Path=CornerRadius,
                                                               Converter={StaticResource CornerRadiusConverter},
                                                               ConverterParameter=BottomLeftRight}">
                                    <ContentPresenter x:Name="ContentElement"
                                                      Margin="{TemplateBinding Padding}"
                                                      telerik:CornerRadiusHelper.ClipRadius="{Binding ElementName=ContentOuterBorder,
                                                                                                      Path=CornerRadius}"
                                                      telerik:CornerRadiusHelper.ClipRadiusOffset="{TemplateBinding telerik:CornerRadiusHelper.ClipRadiusOffset}"
                                                      Content="{TemplateBinding Content}"
                                                      ContentTemplate="{TemplateBinding ContentTemplate}" />
                                </Border>
                            </Grid>
                        </AdornerDecorator>
                        <ResizeGrip x:Name="WindowResizeGrip"
                                    Margin="0,0,1,1"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Bottom"
                                    IsTabStop="False"
                                    Visibility="Collapsed" />
                    </Grid>
                    <!--  RedminePowerカスタマイズ箇所↓↓  -->
                    <!--  対応内容：#165 Window枠に影をつける  -->
                </Border>
            </Border>
            <!--  RedminePowerカスタマイズ箇所↑↑  -->
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="WindowState" Value="Normal">
                <Setter TargetName="LayoutRoot" Property="Margin" Value="1" />
            </Trigger>
            <Trigger Property="WindowState" Value="Maximized">
                <!--  RedminePowerカスタマイズ箇所↓↓  -->
                <!--  対応内容：#165 Window枠に影をつける  -->
                <Setter TargetName="ShadowBorder" Property="Margin" Value="0" />
                <!--<Setter TargetName="Shadow" Property="CornerRadius" Value="0" />-->
                <!--  RedminePowerカスタマイズ箇所↑↑  -->
                <Setter TargetName="ContentOuterBorder" Property="CornerRadius" Value="0" />
                <Setter TargetName="Header" Property="CornerRadius" Value="0" />
            </Trigger>
            <Trigger Property="WindowState" Value="Minimized">
                <Setter TargetName="HeaderContent" Property="Margin" Value="5,0" />
                <Setter TargetName="LayoutRoot" Property="HorizontalAlignment" Value="Left" />
                <Setter TargetName="LayoutRoot" Property="VerticalAlignment" Value="Top" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="IconContent" Property="Content" Value="{x:Null}" />
                    <Condition SourceName="IconContent" Property="ContentTemplate" Value="{x:Null}" />
                </MultiTrigger.Conditions>
                <Setter TargetName="IconContent" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="ResizeMode" Value="CanResizeWithGrip" />
                    <Condition Property="WindowState" Value="Normal" />
                </MultiTrigger.Conditions>
                <Setter Property="MinHeight" Value="44" />
                <Setter TargetName="WindowResizeGrip" Property="Visibility" Value="Visible" />
            </MultiTrigger>
            <Trigger Property="IsInActiveState" Value="False">
                <Setter TargetName="HeaderContent" Property="Opacity" Value="0.7" />
                <Setter TargetName="PART_HeaderButtonsBorder" Property="Opacity" Value="0.7" />
            </Trigger>
            <Trigger Property="mat:MaterialAssist.IsShadowDisabled" Value="True">
                <Setter TargetName="LayoutRoot" Property="Margin" Value="0" />
            </Trigger>
            <!--  RedminePowerカスタマイズ箇所↓↓  -->
            <!--  対応内容：#165 Window枠に影をつける  -->
            <!--<Trigger Property="CornerRadius" Value="0">
                <Setter TargetName="Shadow" Property="Margin" Value="0" />
            </Trigger>-->
            <!--  RedminePowerカスタマイズ箇所↑↑  -->
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadWindowStyle"
           TargetType="telerikNavigation:RadWindow">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderBackground" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="KeyboardNavigation.IsTabStop" Value="False" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="MinHeight" Value="28" />
        <Setter Property="MinWidth" Value="110" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="Header" Value="RadWindow" />
        <Setter Property="WindowStartupLocation" Value="Manual" />
        <Setter Property="WindowState" Value="Normal" />
        <!--  RedminePowerカスタマイズ箇所↓↓  -->
        <!--  対応内容：#168 Windowの標準のタイトルバー領域の高さ  -->
        <Setter Property="CaptionHeight" Value="38.5" />
        <!--  RedminePowerカスタマイズ箇所↑↑  -->
        <Setter Property="ResizeBorder" Value="6" />
        <Setter Property="Template" Value="{StaticResource RadWindowTemplate}" />
        <Setter Property="ModalBackground" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="telerik:CornerRadiusHelper.ClipRadiusOffset" Value="-1" />
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:AnimationGroup AnimationName="Show">
                        <animation:FadeAnimation Direction="In"
                                                 SpeedRatio="3"
                                                 TargetElementName="LayoutRoot" />
                    </animation:AnimationGroup>
                    <animation:AnimationGroup AnimationName="Hide">
                        <animation:FadeAnimation Direction="Out"
                                                 SpeedRatio="3"
                                                 TargetElementName="LayoutRoot" />
                    </animation:AnimationGroup>
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="navigation:RadWindowInteropHelper.OpaqueWindowBackground" Value="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}" />
        <Style.Triggers>
            <Trigger Property="navigation:RadWindowInteropHelper.AllowTransparency" Value="False">
                <Setter Property="navigation:RadWindowInteropHelper.ClipMaskCornerRadius" Value="0" />
                <Setter Property="animation:AnimationManager.IsAnimationEnabled" Value="False" />
                <Setter Property="mat:MaterialAssist.IsShadowDisabled" Value="True" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="RadPromptStyle"
           TargetType="telerikNavigation:RadPrompt">
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="IconTemplate" Value="{StaticResource ConfirmIconTemplate}" />
        <Setter Property="Width" Value="370" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadPrompt">
                    <Grid x:Name="LayoutRoot">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <ContentPresenter x:Name="Icon"
                                          Margin="10,28,10,0"
                                          VerticalAlignment="Center"
                                          ContentTemplate="{TemplateBinding IconTemplate}" />
                        <StackPanel x:Name="PromptContent"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,10">
                            <ScrollViewer Grid.Row="0"
                                          Grid.Column="1"
                                          BorderBrush="{x:Null}"
                                          HorizontalScrollBarVisibility="Auto"
                                          IsTabStop="False"
                                          VerticalScrollBarVisibility="Auto">
                                <ContentPresenter x:Name="PromptText"
                                                  Margin="0,0,10,10" />
                            </ScrollViewer>
                            <TextBox x:Name="PromptTextBox"
                                     Margin="0,0,10,0"
                                     Focusable="True"
                                     IsTabStop="True"
                                     TabIndex="0" />
                        </StackPanel>
                        <StackPanel x:Name="DialogButtons"
                                    Grid.Row="1"
                                    Grid.ColumnSpan="2"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal">
                            <telerik:RadButton x:Name="OK"
                                               MinWidth="70"
                                               Margin="0,10,10,10"
                                               HorizontalAlignment="Right"
                                               telerik:LocalizationManager.ResourceKey="Ok"
                                               telerikNavigation:RadWindow.ResponseButton="Accept"
                                               Command="{x:Static telerik:WindowCommands.Confirm}"
                                               IsTabStop="True"
                                               TabIndex="1" />
                            <telerik:RadButton x:Name="Cancel"
                                               MinWidth="70"
                                               Margin="0,10,10,10"
                                               HorizontalAlignment="Right"
                                               telerik:LocalizationManager.ResourceKey="Cancel"
                                               telerikNavigation:RadWindow.ResponseButton="Cancel"
                                               Command="{x:Static telerik:WindowCommands.Cancel}"
                                               IsTabStop="True"
                                               TabIndex="2" />
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadConfirmStyle"
           TargetType="telerikNavigation:RadConfirm">
        <Setter Property="ContentAreaMargin" Value="0 0 10 0" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="IsTabStop" Value="false" />
        <Setter Property="MinWidth" Value="275" />
        <Setter Property="MaxWidth" Value="500" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="IconTemplate" Value="{StaticResource ConfirmIconTemplate}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadConfirm">
                    <Grid x:Name="LayoutRoot"
                          Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <ContentPresenter x:Name="Icon"
                                          Margin="10"
                                          VerticalAlignment="Top"
                                          ContentTemplate="{TemplateBinding IconTemplate}" />
                        <ScrollViewer Grid.Row="0"
                                      Grid.Column="1"
                                      Margin="0,8"
                                      BorderBrush="{x:Null}"
                                      HorizontalScrollBarVisibility="Auto"
                                      IsTabStop="False"
                                      VerticalScrollBarVisibility="Auto">
                            <ContentPresenter x:Name="ConfirmText"
                                              Margin="{TemplateBinding ContentAreaMargin}" />
                        </ScrollViewer>
                        <StackPanel x:Name="DialogButtons"
                                    Grid.Row="1"
                                    Grid.ColumnSpan="2"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal">
                            <telerik:RadButton x:Name="OK"
                                               MinWidth="70"
                                               Margin="0,10,10,10"
                                               HorizontalAlignment="Right"
                                               telerik:LocalizationManager.ResourceKey="Ok"
                                               telerikNavigation:RadWindow.ResponseButton="Accept"
                                               Command="{x:Static telerik:WindowCommands.Confirm}" />
                            <telerik:RadButton x:Name="Cancel"
                                               MinWidth="70"
                                               Margin="0,10,10,10"
                                               HorizontalAlignment="Right"
                                               telerik:LocalizationManager.ResourceKey="Cancel"
                                               telerikNavigation:RadWindow.ResponseButton="Cancel"
                                               Command="{x:Static telerik:WindowCommands.Cancel}" />
                        </StackPanel>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IconTemplate" Value="{x:Null}">
                            <Setter TargetName="Icon" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadAlertStyle"
           TargetType="telerikNavigation:RadAlert">
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="MinWidth" Value="275" />
        <Setter Property="MaxWidth" Value="500" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="IconTemplate" Value="{StaticResource AlertIconTemplate}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadAlert">
                    <Grid x:Name="LayoutRoot"
                          Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.InputBindings>
                            <KeyBinding Key="Escape"
                                        Command="{x:Static telerik:WindowCommands.Confirm}" />
                        </Grid.InputBindings>
                        <ContentPresenter x:Name="Icon"
                                          Margin="10"
                                          VerticalAlignment="Top"
                                          ContentTemplate="{TemplateBinding IconTemplate}" />
                        <ScrollViewer Grid.Row="0"
                                      Grid.Column="1"
                                      Margin="0,8"
                                      BorderBrush="{x:Null}"
                                      HorizontalScrollBarVisibility="Auto"
                                      IsTabStop="False"
                                      VerticalScrollBarVisibility="Auto">
                            <ContentPresenter x:Name="AlertText"
                                              Margin="0,0,10,0" />
                        </ScrollViewer>
                        <telerik:RadButton x:Name="OK"
                                           Grid.Row="1"
                                           Grid.ColumnSpan="2"
                                           MinWidth="60"
                                           Margin="0,10,10,10"
                                           HorizontalAlignment="Right"
                                           telerik:LocalizationManager.ResourceKey="Ok"
                                           telerikNavigation:RadWindow.ResponseButton="Accept"
                                           Command="{x:Static telerik:WindowCommands.Confirm}" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style BasedOn="{StaticResource RadAlertStyle}"
           TargetType="telerikNavigation:RadAlert" />
    <Style BasedOn="{StaticResource RadPromptStyle}"
           TargetType="telerikNavigation:RadPrompt" />
    <Style BasedOn="{StaticResource RadConfirmStyle}"
           TargetType="telerikNavigation:RadConfirm" />

    <!--  Dialogs in RadScheduleView  -->
    <Style x:Key="RadSchedulerWindowStyle"
           BasedOn="{StaticResource RadWindowStyle}"
           TargetType="telerikNavigation:RadWindow" />
    <Style BasedOn="{StaticResource RadSchedulerWindowStyle}"
           TargetType="telerikScheduleView:SchedulerWindow" />
    <Style BasedOn="{StaticResource RadDurationPickerStyle}"
           TargetType="telerikScheduleView:DurationPicker" />
</ResourceDictionary>
