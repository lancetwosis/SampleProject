                                           Grid.Column="1"
                                           Grid.ColumnSpan="2"
                                           Margin="{StaticResource ScheduleViewDialogElementOffset}"
                                           VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                           AutomationProperties.AutomationId="EndTimeDateTimePicker"
                                           AutomationProperties.Name="EndTime"
                                           Culture="{Binding DefaultCulture,
                                                             Source={StaticResource LocalizationManager}}"
                                           InputMode="{Binding IsAllDayEvent,
                                                               Converter={StaticResource BoolToInputModeConverter},
                                                               ConverterParameter={x:Static telerik:InputMode.DatePicker}}"
                                           IsEnabled="{Binding IsNotRecurrent}"
