                                                            TypeName="RadRibbonToggleButton" />
                    <telerikRibbonView:CustomContainerStyle ContainerStyle="{StaticResource QATRibbonRadioButtonStyle}"
                                                            TypeName="RadRibbonRadioButton" />
                    <telerikRibbonView:CustomContainerStyle ContainerStyle="{StaticResource QATRibbonDropDownButtonStyle}"
