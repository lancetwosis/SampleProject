    <ControlTemplate x:Key="OverflowMenuItemTemplate"
                     TargetType="telerikNavigation:RadMenuItem">
        <ContentPresenter Content="{TemplateBinding Header}" />
    </ControlTemplate>
    <ControlTemplate x:Key="QuickAccessToolBarTemplate"
                     TargetType="telerikRibbonView:QuickAccessToolBar">
        <Grid x:Name="RootElement"
              Margin="{TemplateBinding Padding}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid x:Name="BelowBackgroundGrid"
                  Grid.ColumnSpan="3"
                  Visibility="Collapsed">
                <Border Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="1,0,1,1" />
            </Grid>
            <Grid x:Name="AboveBackgroundGridWrapper">
                <Grid x:Name="AboveBackgroundGrid" />
            </Grid>
            <telerikToolBar:RadToolBarPanel x:Name="PART_StripPanel"
                                            Orientation="Horizontal" />
            <telerikRibbonView:RadRibbonDropDownButton x:Name="DropDownButton"
                                                       Grid.Column="1"
                                                       Padding="0"
                                                       VerticalAlignment="Center"
                                                       helpers:ThemeHelper.MouseOverBrush="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}"
                                                       helpers:ThemeHelper.PressedBrush="{telerik1:Office2019Resource ResourceKey=AccentPressedBackgroundBrush}"
                                                       DropDownIndicatorVisibility="Collapsed"
                                                       Foreground="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}"
                                                       IsBackgroundVisible="False"
                                                       IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                        Path=IsOverflowOpen,
                                                                        Mode=TwoWay}"
                                                       Visibility="{TemplateBinding CustomizationMenuVisibility}">
                <ToolTipService.ToolTip>
                    <TextBlock Text="{telerik:LocalizableResource Key=RibbonViewQATCustomize}" />
                </ToolTipService.ToolTip>
                <telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
                    <telerikNavigation:RadMenuGroupItem Background="Transparent"
                                                        BorderThickness="0">
                        <telerikRibbonView:RadGroupHeader Margin="-1"
                                                          Content="{telerik:LocalizableResource Key=RibbonViewQATCustomize}"
                                                          IsTabStop="False" />
                        <telerikNavigation:RadMenuItem SubmenuItemTemplateKey="{StaticResource OverflowMenuItemTemplate}"
                                                       Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                            Path=HasOverflowItems,
                                                                            Converter={StaticResource RibbonBooleanToVisibilityConverter}}">
                            <telerikNavigation:RadMenuItem.Header>
                                <telerikToolBar:RadToolBarPanel x:Name="PART_OverflowPanel"
                                                                VerticalAlignment="Top"
                                                                Orientation="Vertical" />
                            </telerikNavigation:RadMenuItem.Header>
                        </telerikNavigation:RadMenuItem>
                        <telerikNavigation:RadMenuItem Padding="0"
                                                       IsSeparator="True"
                                                       Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                            Path=HasOverflowItems,
                                                                            Converter={StaticResource RibbonBooleanToVisibilityConverter}}" />
                        <telerikNavigation:RadMenuItem x:Name="TogglePositionMenuItem"
                                                       Command="{x:Static telerikRibbonView:RibbonCommands.ToggleQATPositionCommand}"
                                                       Header="{telerik:LocalizableResource Key=RibbonViewQATShowBelow}" />
                        <telerikNavigation:RadMenuItem Padding="0"
                                                       IsSeparator="True" />
                        <telerikNavigation:RadMenuItem x:Name="MinimizeMenuItem"
                                                       Command="{x:Static telerikRibbonView:RibbonCommands.MinimizeRibbonCommand}"
                                                       Header="{telerik:LocalizableResource Key=RibbonViewQATMinimize}" />
                    </telerikNavigation:RadMenuGroupItem>
                </telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
                <telerik:RadGlyph x:Name="DropDownIcon"
                                  HorizontalAlignment="Center"
                                  VerticalAlignment="Center"
                                  FontSize="16"
                                  Foreground="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}"
                                  Glyph="{StaticResource GlyphArrowOverflowDown}" />
            </telerikRibbonView:RadRibbonDropDownButton>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStateGroup">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="RootElement"
                                             Storyboard.TargetProperty="Opacity"
                                             To="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}"
                                             Duration="0:0:0.2" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="HostingState">
                    <VisualState x:Name="NotHosted" />
                    <VisualState x:Name="HostedAboveRibbon" />
                    <VisualState x:Name="HostedBelowRibbon">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="AboveBackgroundGrid"
                                                           Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BelowBackgroundGrid"
                                                           Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_StripPanel"
                                                           Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>2 2 0 2</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="TogglePositionMenuItem"
                                                           Storyboard.TargetProperty="Header">
                                <DiscreteObjectKeyFrame KeyTime="0"
                                                        Value="{telerik:LocalizableResource Key=RibbonViewQATShowAbove}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="MinimizeStates">
                    <VisualState x:Name="NotMinimized" />
                    <VisualState x:Name="Minimized">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="MinimizeMenuItem"
                                                           Storyboard.TargetProperty="Header">
                                <DiscreteObjectKeyFrame KeyTime="0"
                                                        Value="{telerik:LocalizableResource Key=RibbonViewQATRestore}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger SourceName="DropDownButton" Property="IsMouseOver" Value="True">
