            <Trigger SourceName="DialogLauncher" Property="IsMouseOver" Value="True">
                <Setter TargetName="DialogLauncher" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
            </Trigger>
            <Trigger SourceName="DialogLauncher" Property="IsPressed" Value="True">
                <Setter TargetName="DialogLauncher" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Content" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=DisabledBorderBrush}" />
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledForegroundBrush}" />
                <Setter TargetName="DialogLauncher" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledIconBrush}" />
