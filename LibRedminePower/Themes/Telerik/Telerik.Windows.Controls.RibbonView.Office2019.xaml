                <ContentControl x:Name="HeaderElement"
                                Margin="{TemplateBinding Padding}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                Content="{TemplateBinding Header}"
                                ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                                ContentTemplate="{TemplateBinding HeaderTemplate}"
                                ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                Foreground="{TemplateBinding Foreground}"
                                IsTabStop="False" />
            </Grid>
            <Border x:Name="SelectedVisual"
                    Width="{Binding ElementName=HeaderElement,
                                    Path=ActualWidth}"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent},
                                          Path=(helpers:ThemeHelper.CheckedBrush)}"
                    BorderThickness="0,0,0,3"
                    RenderTransformOrigin="0.5 1">
                <Border.RenderTransform>
                    <ScaleTransform ScaleX="0" ScaleY="1" />
                </Border.RenderTransform>
            </Border>
            <Border x:Name="ContentPanel"
                    Width="0"
                    Height="0"
                    Background="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}"
                    BorderBrush="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}"
                    BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent},
                                              Path=BorderThickness,
                                              Converter={StaticResource ThicknessToOrientedThicknessConverter},
                                              ConverterParameter=LeftRightBottom}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <telerikRibbonViewPrimitives:RibbonScrollViewer x:Name="TabItemsScrollViewer"
                                                                    Padding="1"
                                                                    VerticalContentAlignment="Stretch">
                        <ItemsPresenter HorizontalAlignment="Left" />
                    </telerikRibbonViewPrimitives:RibbonScrollViewer>
                    <StackPanel Grid.Column="1">
                        <telerikRibbonView:RadRibbonToggleButton x:Name="MinimizeButton"
                                                                 MinWidth="26"
                                                                 MinHeight="26"
                                                                 Margin="0"
                                                                 IsChecked="{Binding RelativeSource={RelativeSource AncestorType={x:Type telerik:RadRibbonView}},
                                                                                     Path=IsMinimized,
                                                                                     Mode=TwoWay}"
                                                                 IsEnabled="{Binding IsMinimizable,
                                                                                     RelativeSource={RelativeSource AncestorType={x:Type telerik:RadRibbonView}}}"
                                                                 Style="{Binding MinimizeButtonStyle,
                                                                                 RelativeSource={RelativeSource AncestorType={x:Type telerik:RadRibbonView}}}"
                                                                 Visibility="{Binding MinimizeButtonVisibility,
                                                                                      RelativeSource={RelativeSource AncestorType={x:Type telerik:RadRibbonView}}}" />
                    </StackPanel>
                </Grid>
            </Border>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="ContextualGroupStates">
                    <VisualState x:Name="DefaultContextualGroupTab" />
                    <VisualState x:Name="IsSingleTabInContextualGroup">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BorderVisual"
                                                           Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="IsLastTabInContextualGroup">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BorderVisual"
                                                           Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="KeyboardNavigationVisual"
                                                           Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="IsFirstTabInContextualGroup">
                        <Storyboard />
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="KeyboardNavigationStates">
                    <VisualState x:Name="KeyboardUnfocused" />
                    <VisualState x:Name="KeyboardFocused">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="HeaderElement"
                                                           Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0"
                                                        Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
                            </ObjectAnimationUsingKeyFrames>
                            <DoubleAnimation Storyboard.TargetName="KeyboardNavigationVisual"
                                             Storyboard.TargetProperty="Opacity"
                                             To="1"
                                             Duration="0" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger SourceName="HeaderElement" Property="IsMouseOver" Value="True">
                <Setter TargetName="MouseOverVisual" Property="Opacity" Value="0.5" />
                <Setter TargetName="SelectedVisual" Property="Width" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ActualWidth}" />
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="SelectedVisual"
                                                           Storyboard.TargetProperty="RenderTransform.ScaleX">
                                <EasingDoubleKeyFrame KeyTime="0"
                                                      Value="0" />
                                <EasingDoubleKeyFrame KeyTime="0:0:0.12"
                                                      Value="1">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseIn" />
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="SelectedVisual"
                                                           Storyboard.TargetProperty="RenderTransform.ScaleX">
                                <EasingDoubleKeyFrame KeyTime="0"
                                                      Value="1" />
                                <EasingDoubleKeyFrame KeyTime="0:0:0.1"
                                                      Value="0">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseIn" />
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
                <Setter TargetName="HeaderElement" Property="Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IsSelected" Value="False">
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="0" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="HeaderElement" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                <Setter TargetName="BorderVisual" Property="Opacity" Value="0.1" />
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledForegroundBrush}" />
            </Trigger>
            <Trigger Property="IsContextualTabVisible" Value="True">
                <Setter TargetName="HeaderPanel" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsContextualTabVisible" Value="False">
                <Setter TargetName="HeaderPanel" Property="Visibility" Value="Collapsed" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="ContextualTabsStyle"
           TargetType="telerikRibbonView:RadRibbonTab">
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="helpers:ThemeHelper.CheckedBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="BorderThickness" Value="{telerik1:Office2019Resource ResourceKey=HeaderBorderThickness}" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="UseLayoutRounding" Value="True" />
        <Setter Property="Padding" Value="5,0" />
        <Setter Property="MinHeight" Value="26" />
        <Setter Property="VerticalAlignment" Value="Stretch" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusVisualStyle">
            <Setter.Value>
                <Style TargetType="Control">
                    <Setter Property="Visibility" Value="Collapsed" />
                </Style>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikRibbonViewPrimitives:RibbonGroupsPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource ContextualTabsTemplate}" />
    </Style>
    <ControlTemplate x:Key="ContextualGroupsTemplate"
                     TargetType="telerikRibbonView:RadRibbonContextualGroup">
        <Grid x:Name="HeaderGrid"
              Background="Transparent">
            <Border BorderBrush="{TemplateBinding Color}"
                    BorderThickness="{TemplateBinding BorderThickness}" />
            <Border Background="{TemplateBinding Color}"
                    OpacityMask="#4C000000" />
            <ContentControl Margin="{TemplateBinding Padding}"
                            HorizontalAlignment="{TemplateBinding HeaderAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            Content="{TemplateBinding Header}"
                            ContentTemplate="{TemplateBinding HeaderTemplate}"
                            FontSize="{TemplateBinding FontSize}"
                            Foreground="{TemplateBinding Foreground}"
                            IsTabStop="False" />
        </Grid>
    </ControlTemplate>
    <Style x:Key="ContextualGroupsStyle"
           TargetType="telerikRibbonView:RadRibbonContextualGroup">
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HeaderAlignment" Value="Center" />
        <Setter Property="Padding" Value="5,0" />
        <Setter Property="Margin" Value="0,0,3,0" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="UseLayoutRounding" Value="True" />
        <Setter Property="Template" Value="{StaticResource ContextualGroupsTemplate}" />
    </Style>
    <Style x:Key="WindowTitleStyle"
           TargetType="telerikRibbonViewPrimitives:WindowTitle">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <!--  RedminePowerカスタマイズ箇所↓↓  -->
        <!--  対応内容：#235 RibbonWindowのタイトルバーの違和感を改善  -->
        <Setter Property="Foreground" Value="Black" />
        <!--  RedminePowerカスタマイズ箇所↑↑  -->
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="IsHitTestVisible" Value="False" />
        <Setter Property="IsActive" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonViewPrimitives:WindowTitle">
                    <!--  RedminePowerカスタマイズ箇所↓↓  -->
                    <!--  対応内容：#235 RibbonWindowのタイトルバーの違和感を改善 Marginの調整  -->
                    <telerikRibbonViewPrimitives:WindowTitlePanel Margin="125,0,0,0">
                        <!--  RedminePowerカスタマイズ箇所↑↑  -->
                        <TextBlock x:Name="Title"
                                   FontSize="{telerik1:Office2019Resource ResourceKey=FontSize}"
                                   Foreground="{TemplateBinding Foreground}"
                                   Text="{TemplateBinding Title}"
                                   TextTrimming="CharacterEllipsis" />
                        <TextBlock x:Name="Divider"
                                   FontSize="{telerik1:Office2019Resource ResourceKey=FontSize}"
                                   Foreground="{TemplateBinding Foreground}"
                                   Text="{telerik:LocalizableResource Key=RibbonViewWindowTitleDivider}"
                                   Visibility="{TemplateBinding DividerVisibility}" />
                        <TextBlock x:Name="ApplicationName"
                                   FontSize="{telerik1:Office2019Resource ResourceKey=FontSize}"
                                   Foreground="{TemplateBinding Foreground}"
                                   Text="{TemplateBinding ApplicationName}"
                                   TextTrimming="CharacterEllipsis" />
                    </telerikRibbonViewPrimitives:WindowTitlePanel>
                    <ControlTemplate.Triggers>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=IsHostedInRibbonWindow, FallbackValue=False}" Value="True" />
                                <Condition Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonWindow}, Path=IsWindowsThemeEnabled}" Value="True" />
                                <Condition Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonWindow}, Path=IsTitleVisible}" Value="False" />
                            </MultiDataTrigger.Conditions>
                            <Setter Property="Foreground" Value="White" />
                        </MultiDataTrigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="HelpButtonStyle"
           BasedOn="{StaticResource RadRibbonButtonStyle}"
           TargetType="telerikRibbonView:RadRibbonButton">
        <Setter Property="Padding" Value="0" />
        <Setter Property="MinWidth" Value="26" />
        <Setter Property="MinHeight" Value="26" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="IsTabStop" Value="False" />
    </Style>
    <ControlTemplate x:Key="MinimizeButtonTemplate"
                     TargetType="telerikRibbonView:RadRibbonToggleButton">
        <Grid x:Name="RootElement">
            <Border x:Name="BorderVisual"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}" />
            <Border x:Name="KeyboardNavigationVisual"
                    Background="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}"
                    BorderBrush="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Opacity="0" />
            <telerik:RadGlyph x:Name="MinimizeButtonContent"
                              Margin="{TemplateBinding Padding}"
                              FontSize="16"
                              Glyph="{StaticResource GlyphArrowChevronUp}" />
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="KeyboardNavigationStates">
                    <VisualState x:Name="KeyboardUnfocused" />
                    <VisualState x:Name="KeyboardFocused">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="KeyboardNavigationVisual"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1"
                                             Duration="0" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="MinimizeButtonContent" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledIconBrush}" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush), Mode=OneWay}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBrush), Mode=OneWay}" />
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="MinimizeButtonContent" Property="Glyph" Value="{StaticResource GlyphUnpin}" />
                <Setter TargetName="RootElement" Property="telerikRibbonView:ScreenTip.Title" Value="{telerik:LocalizableResource Key=RibbonViewExpandRibbon}" />
            </Trigger>
            <Trigger Property="IsChecked" Value="False">
                <Setter TargetName="RootElement" Property="telerikRibbonView:ScreenTip.Title" Value="{telerik:LocalizableResource Key=RibbonViewMinimizeRibbon}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="MinimizeButtonStyle"
           BasedOn="{StaticResource RadRibbonToggleButtonStyle}"
           TargetType="telerikRibbonView:RadRibbonToggleButton">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="VerticalAlignment" Value="Bottom" />
        <Setter Property="Size" Value="Small" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="Margin" Value="2" />
        <Setter Property="MinWidth" Value="16" />
        <Setter Property="MinHeight" Value="16" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=IconBrush}" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="Template" Value="{StaticResource MinimizeButtonTemplate}" />
    </Style>
    <ControlTemplate x:Key="RadRibbonViewTemplate"
                     TargetType="telerikRibbonView:RadRibbonView">
        <Grid x:Name="RootPanel"
              Margin="{TemplateBinding Padding}">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid Grid.Row="1">
                <Grid.Resources>
                    <Style x:Key="SelectedTabContentStyle"
                           TargetType="ContentPresenter">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView},
                                                           Path=LayoutMode}"
                                         Value="Simplified">
                                <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=SimplifiedContentHeight}" />
                            </DataTrigger>
                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView},
                                                           Path=LayoutMode}"
                                         Value="Default">
                                <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=ContentHeight}" />
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </Grid.Resources>
                <ContentPresenter x:Name="SelectedTabContent"
                                  Style="{StaticResource SelectedTabContentStyle}" />
            </Grid>
            <Grid x:Name="TopGrid"
                  MinHeight="26">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Border x:Name="titleBarBackground"
                        Grid.ColumnSpan="3"
                        Background="{TemplateBinding TitleBarBackground}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{telerik1:Office2019Resource ResourceKey=HeaderBorderThickness}"
                        IsHitTestVisible="False" />
                <Border x:Name="belowTitleBarBorder"
                        Grid.Row="1"
                        Grid.ColumnSpan="3"
                        Background="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="1,0" />
                <telerikRibbonViewPrimitives:RibbonTitleBarPanel x:Name="RibbonTitleBarPanel"
                                                                 Grid.ColumnSpan="3"
                                                                 MinHeight="32"
                                                                 Visibility="{TemplateBinding TitleBarVisibility}">
                    <ContentPresenter x:Name="AboveToolBarHost"
                                      VerticalAlignment="Center"
                                      telerikRibbonViewPrimitives:RibbonTitleBarPanel.Element="QuickAccessToolBar" />
                    <telerikRibbonViewPrimitives:WindowTitle x:Name="WindowTitle"
                                                             Title="{TemplateBinding Title}"
                                                             telerikRibbonViewPrimitives:RibbonTitleBarPanel.Element="WindowTitle"
                                                             ApplicationName="{TemplateBinding ApplicationName}"
                                                             Style="{TemplateBinding WindowTitleStyle}" />
                    <StackPanel x:Name="ContextualGroupsHeaders"
                                telerikRibbonViewPrimitives:RibbonTitleBarPanel.Element="ContextualGroupsPanel"
                                Orientation="Horizontal" />
                </telerikRibbonViewPrimitives:RibbonTitleBarPanel>
                <telerikRibbonViewPrimitives:RadRibbonApplicationDropDownButton x:Name="ApplicationButton"
                                                                                Grid.Row="1"
                                                                                Content="{TemplateBinding ApplicationButtonContent}"
                                                                                IsEnabled="{TemplateBinding IsApplicationButtonEnabled}"
                                                                                IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                                 Path=IsApplicationMenuOpen,
                                                                                                 Mode=TwoWay}"
                                                                                SmallImage="{TemplateBinding ApplicationButtonImageSource}"
                                                                                Style="{TemplateBinding ApplicationButtonStyle}"
                                                                                Visibility="{TemplateBinding ApplicationButtonVisibility}" />
                <telerikRibbonViewPrimitives:RibbonScrollViewer x:Name="TabsScrollViewer"
                                                                Grid.Row="1"
                                                                Grid.Column="1">
                    <ItemsPresenter />
                </telerikRibbonViewPrimitives:RibbonScrollViewer>
                <StackPanel Grid.Row="1"
                            Grid.Column="2"
                            Orientation="Horizontal">
                    <telerik:RadToggleSwitchButton x:Name="LayoutModeButton"
                                                   Margin="0,0,0,3"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   CheckedContent="{TemplateBinding LayoutModeButtonContent}"
                                                   ContentPosition="Right"
                                                   IsChecked="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView},
                                                                       Path=LayoutMode,
                                                                       Converter={StaticResource BooleanToRibbonLayoutConverter},
                                                                       Mode=TwoWay}"
                                                   UncheckedContent="{TemplateBinding LayoutModeButtonContent}"
                                                   Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                        Path=ShowLayoutModeButton,
                                                                        Converter={StaticResource RibbonBooleanToVisibilityConverter}}" />
                    <telerikRibbonView:RadRibbonButton x:Name="HelpButton"
                                                       telerikRibbonView:ScreenTip.Title="{telerik:LocalizableResource Key=RibbonViewHelp}"
                                                       Command="{TemplateBinding HelpCommand}"
                                                       CommandParameter="{TemplateBinding HelpCommandParameter}"
                                                       CommandTarget="{TemplateBinding HelpCommandTarget}"
                                                       SmallImage="{TemplateBinding HelpButtonImageSource}"
                                                       Style="{TemplateBinding HelpButtonStyle}"
                                                       Visibility="{TemplateBinding HelpButtonVisibility}" />
                    <ContentPresenter Content="{TemplateBinding TabStripAdditionalContent}" />
                </StackPanel>
            </Grid>
            <ContentPresenter x:Name="BelowToolBarHost"
                              Grid.Row="2" />
            <Popup x:Name="TabContentPopup"
                   AllowsTransparency="True"
                   PlacementTarget="{Binding ElementName=TopGrid}">
                <Grid x:Name="PopupGrid"
                      Margin="0,0,0,5">
                    <Grid.Resources>
                        <Style x:Key="SelectedTabContentPopupStyle"
                               TargetType="ContentPresenter">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView},
                                                               Path=LayoutMode}"
                                             Value="Simplified">
                                    <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=SimplifiedContentHeight}" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView},
                                                               Path=LayoutMode}"
                                             Value="Default">
                                    <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=ContentHeight}" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Grid.Resources>
                    <ContentPresenter x:Name="SelectedTabContentPopup"
                                      Style="{StaticResource SelectedTabContentPopupStyle}" />
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsHostedInRibbonWindow" Value="true">
                <Setter TargetName="titleBarBackground" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="RibbonTitleBarPanel" Property="MinHeight" Value="26" />
                <Setter TargetName="belowTitleBarBorder" Property="BorderThickness" Value="0" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="ContextualGroupsHeaders" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=DisabledBorderBrush}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadRibbonViewStyle"
           TargetType="telerikRibbonView:RadRibbonView">
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikRibbonViewPrimitives:RibbonTabStripPanel MinTabWidth="70" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="UseLayoutRounding" Value="True" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusManager.IsFocusScope" Value="True" />
        <Setter Property="KeyboardNavigation.DirectionalNavigation" Value="None" />
        <Setter Property="KeyboardNavigation.ControlTabNavigation" Value="None" />
        <Setter Property="KeyboardNavigation.TabNavigation" Value="None" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="TitleBarBackground" Value="{telerik1:Office2019Resource ResourceKey=HeaderBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="ContentHeight" Value="95" />
