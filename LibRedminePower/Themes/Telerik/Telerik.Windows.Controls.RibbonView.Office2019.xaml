                            <Border.Effect>
                                <DropShadowEffect BlurRadius="4"
                                                  Opacity="0.8"
                                                  ShadowDepth="0"
                                                  Color="Black" />
                            </Border.Effect>
                            <Border x:Name="outerBorder"
                                    Background="{TemplateBinding WindowBackground}"
                                    BorderBrush="Black"
                                    BorderThickness="0.5"
                                    ClipToBounds="True">
