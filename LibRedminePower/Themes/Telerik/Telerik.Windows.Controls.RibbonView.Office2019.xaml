                <Setter TargetName="ButtonPart" Property="Background" Value="{telerik1:Office2019Resource ResourceKey=ButtonBackgroundBrush}" />
                <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
            </Trigger>
            <Trigger SourceName="DropDownPart" Property="IsChecked" Value="True">
                <Setter TargetName="DropDownIndicator" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=ButtonBackgroundBrush}" />
                <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="DropDownPart" Property="IsChecked" Value="True" />
                    <Condition Property="CurrentSize" Value="Medium" />
                    <Condition Property="IsTextInMiddleButton" Value="False" />
                </MultiTrigger.Conditions>
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="DropDownPart" Property="IsMouseOver" Value="True" />
                    <Condition Property="CurrentSize" Value="Medium" />
                    <Condition Property="IsTextInMiddleButton" Value="False" />
                </MultiTrigger.Conditions>
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="DropDownPart" Property="IsChecked" Value="True" />
                    <Condition SourceName="DropDownPart" Property="IsMouseOver" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="ButtonPart" Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsToggle" Value="True" />
                    <Condition Property="IsChecked" Value="True" />
                    <Condition Property="IsOpen" Value="False" />
                    <Condition Property="IsMouseOver" Value="True" />
                </MultiTrigger.Conditions>
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="DropDownIndicator" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MouseOverBackgroundBrush}" />
                <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsToggle" Value="True" />
                    <Condition Property="IsChecked" Value="True" />
                    <Condition Property="IsOpen" Value="False" />
                    <Condition SourceName="ButtonPart" Property="IsMouseOver" Value="True" />
                </MultiTrigger.Conditions>
                <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MouseOverBackgroundBrush}" />
                <Setter TargetName="DropDownPart" Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MouseOverBackgroundBrush}" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsToggle" Value="True" />
                    <Condition Property="IsChecked" Value="True" />
                    <Condition Property="IsOpen" Value="False" />
                    <Condition SourceName="DropDownPart" Property="IsMouseOver" Value="True" />
                </MultiTrigger.Conditions>
                <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MouseOverBackgroundBrush}" />
                <Setter TargetName="ButtonPart" Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MouseOverBackgroundBrush}" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsToggle" Value="True" />
                    <Condition Property="IsChecked" Value="True" />
                    <Condition Property="IsOpen" Value="False" />
                    <Condition SourceName="DropDownPart" Property="IsPressed" Value="True" />
                </MultiTrigger.Conditions>
                <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=PressedBackgroundBrush}" />
                <Setter TargetName="ButtonPart" Property="Background" Value="{telerik1:Office2019Resource ResourceKey=ButtonBackgroundBrush}" />
                <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
