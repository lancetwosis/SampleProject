                                        <RowDefinition Height="28" />
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <!--  RedminePowerカスタマイズ箇所↓↓  -->
                                    <!--  対応内容：#235 RibbonWindowのタイトルバーの違和感を改善  -->
                                    <Border Grid.Row="0"
                                            Grid.Column="1"
                                            Background="White" />
                                    <!--  RedminePowerカスタマイズ箇所↑↑  -->
                                    <Grid Grid.ColumnSpan="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <!--  RedminePowerカスタマイズ箇所↓↓  -->
                                        <!--  対応内容：#235 RibbonWindowのタイトルバーの違和感を改善 Margin修正  -->
                                        <StackPanel x:Name="IconPanel"
                                                    Margin="4,-2,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    Orientation="Horizontal"
                                                    Visibility="{TemplateBinding IconVisibility}">
                                            <!--  RedminePowerカスタマイズ箇所↑↑  -->
                                            <Image Name="PART_Icon"
                                                   Width="{Binding Path=SmallIconSize.Width,
                                                                   Source={x:Static shell:SystemParameters2.Current}}"
                                                   Height="{Binding Path=SmallIconSize.Height,
                                                                    Source={x:Static shell:SystemParameters2.Current}}"
                                                   shell:WindowChrome.IsHitTestVisibleInChrome="True"
                                                   Source="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                    Path=Icon,
                                                                    Converter={StaticResource IconConverter}}" />
                                            <!--  RedminePowerカスタマイズ箇所↓↓  -->
                                            <!--  対応内容：#235 RibbonWindowのタイトルバーの違和感を改善 アイコン右の縦線をコメントアウト  -->
                                            <!--<Rectangle Width="1"
                                                   Margin="4,0"
                                                   Fill="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />-->
                                            <!--  RedminePowerカスタマイズ箇所↑↑  -->
                                        </StackPanel>
                                        <telerikRibbonViewPrimitives:WindowTitle x:Name="WindowTitle"
                                                                                 Title="{TemplateBinding Title}"
                                                                                 Grid.Column="1"
                                                                                 Margin="0,0,65,0"
                                                                                 HorizontalAlignment="Center"
                                                                                 VerticalAlignment="Center"
                                                                                 Style="{TemplateBinding TitleBarStyle}" />
                                    </Grid>
                                    <!--  RedminePowerカスタマイズ箇所↓↓  -->
                                    <!--  対応内容：#235 RibbonWindowのタイトルバーの違和感を改善 Windowボタンの外観修正(最小化・最大化・閉じる)  -->
                                    <!--  対応内容：#235 Margin修正  -->
                                    <StackPanel x:Name="buttonPanel"
                                                Grid.ColumnSpan="3"
                                                Margin="0,-2,0,0"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Center"
                                                Orientation="Horizontal">
                                        <!--  対応内容：#235 Foreground追加  -->
                                        <telerik:RadButton x:Name="minimizeButton"
                                                           shell:WindowChrome.IsHitTestVisibleInChrome="True"
                                                           Command="{x:Static shell:SystemCommands.MinimizeWindowCommand}"
                                                           CornerRadius="{Binding Path=CornerRadius.TopRight,
                                                                                  RelativeSource={RelativeSource TemplatedParent}}"
                                                           Foreground="Black"
                                                           Style="{StaticResource RibbonWindowButtonStyle}"
                                                           ToolTipService.ToolTip="Minimize">
                                            <telerik:RadButton.ToolTip>
                                                <TextBlock Text="{telerik:LocalizableResource Key=RibbonWindowMinimize}" />
                                            </telerik:RadButton.ToolTip>
                                            <!--  対応内容：#235 Margin追加  -->
                                            <telerik:RadGlyph Margin="0,0,0,2"
                                                              FontSize="16"
                                                              Glyph="{StaticResource GlyphMinimize}" />
                                        </telerik:RadButton>
                                        <!--  対応内容：#235 Restoreボタン追加  -->
                                        <telerik:RadToggleButton x:Name="restorButton"
                                                                 shell:WindowChrome.IsHitTestVisibleInChrome="True"
                                                                 Command="{x:Static shell:SystemCommands.RestoreWindowCommand}"
                                                                 CornerRadius="{Binding Path=CornerRadius.TopRight,
                                                                                        RelativeSource={RelativeSource TemplatedParent}}"
                                                                 Foreground="Black"
                                                                 IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                     Path=WindowState,
                                                                                     Converter={StaticResource BooleanToWindowStateConverter},
                                                                                     Mode=TwoWay}"
                                                                 Style="{StaticResource RadRibbonWindowToggleButtonStyle}"
                                                                 Visibility="Collapsed">
                                            <!--  対応内容：#235 ToolTip追加  -->
                                            <telerik:RadToggleButton.ToolTip>
                                                <TextBlock Text="{telerik:LocalizableResource Key=RibbonWindowRestoreDown}" />
                                            </telerik:RadToggleButton.ToolTip>
                                            <telerik:RadGlyph FontSize="16"
                                                              Glyph="{StaticResource GlyphWindowCollapse}" />
                                        </telerik:RadToggleButton>
                                        <!--  対応内容：#235 Foreground追加  -->
                                        <telerik:RadToggleButton x:Name="maximizeButton"
                                                                 shell:WindowChrome.IsHitTestVisibleInChrome="True"
                                                                 CornerRadius="{Binding Path=CornerRadius.TopRight,
                                                                                        RelativeSource={RelativeSource TemplatedParent}}"
                                                                 Foreground="Black"
                                                                 IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                     Path=WindowState,
                                                                                     Converter={StaticResource BooleanToWindowStateConverter},
                                                                                     Mode=TwoWay}"
                                                                 Style="{StaticResource RadRibbonWindowToggleButtonStyle}">
                                            <!--  対応内容：#235 ToolTip追加  -->
                                            <telerik:RadToggleButton.ToolTip>
                                                <TextBlock Text="{telerik:LocalizableResource Key=RibbonWindowMaximize}" />
                                            </telerik:RadToggleButton.ToolTip>
                                            <telerik:RadGlyph FontSize="16"
                                                              Glyph="{StaticResource GlyphWindow}" />
                                        </telerik:RadToggleButton>
                                        <!--  対応内容：#235 Foreground追加  -->
                                        <telerik:RadButton x:Name="closeButton"
                                                           shell:WindowChrome.IsHitTestVisibleInChrome="True"
                                                           Command="{x:Static shell:SystemCommands.CloseWindowCommand}"
                                                           CornerRadius="{Binding Path=CornerRadius.TopRight,
                                                                                  RelativeSource={RelativeSource TemplatedParent}}"
                                                           Foreground="Black">
                                            <!--  対応内容：#235 Style追加  -->
                                            <telerik:RadButton.Style>
                                                <Style BasedOn="{StaticResource RibbonWindowButtonStyle}"
                                                       TargetType="telerik:RadButton">
                                                    <Style.Triggers>
                                                        <Trigger Property="IsMouseOver" Value="True">
                                                            <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="#FFFB3E3E" />
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </telerik:RadButton.Style>
                                            <telerik:RadButton.ToolTip>
                                                <TextBlock Text="{telerik:LocalizableResource Key=RibbonWindowClose}" />
                                            </telerik:RadButton.ToolTip>
                                            <telerik:RadGlyph FontSize="16"
                                                              Glyph="{StaticResource GlyphClose}" />
                                        </telerik:RadButton>
                                    </StackPanel>
                                    <!--  RedminePowerカスタマイズ箇所↑↑  -->
                                    <Border x:Name="PART_ClientAreaBorder"
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Margin="0,12,0,0"
                                            Background="{TemplateBinding Background}" />
                                    <AdornerDecorator x:Name="Adorner"
                                                      Grid.RowSpan="2"
                                                      Grid.Column="1">
                                        <ContentPresenter Name="PART_RootContentPresenter"
                                                          Canvas.ZIndex="0" />
                                    </AdornerDecorator>
                                    <ResizeGrip x:Name="WindowResizeGrip"
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Bottom"
                                                shell:WindowChrome.ResizeGripDirection="BottomRight"
                                                IsTabStop="False"
                                                Visibility="Collapsed" />
                                </Grid>
                            </Border>
                            <!--  RedminePowerカスタマイズ箇所↓↓  -->
                            <!--  対応内容：#235 RibbonWindowのタイトルバーの違和感を改善 影を付ける  -->
                        </Border>
                        <!--  RedminePowerカスタマイズ箇所↑↑  -->
                    </Grid>
                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="Window.ResizeMode" Value="CanResizeWithGrip" />
                                <Condition Property="Window.WindowState" Value="Normal" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="WindowResizeGrip" Property="Visibility" Value="Visible" />
                        </MultiTrigger>
                        <Trigger Property="Window.ResizeMode" Value="NoResize">
                            <Setter TargetName="minimizeButton" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="maximizeButton" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="Window.ResizeMode" Value="CanMinimize">
                            <Setter TargetName="maximizeButton" Property="IsEnabled" Value="False" />
                        </Trigger>
                        <Trigger Property="WindowState" Value="Maximized">
                            <Setter TargetName="MaximizeWindowDecorator" Property="Margin" Value="6" />
                            <Setter Property="CornerRadius" Value="0" />
                            <!--  RedminePowerカスタマイズ箇所↓↓  -->
                            <!--  対応内容：#235 RibbonWindowのタイトルバーの違和感を改善 影を付ける  -->
                            <Setter TargetName="ShadowBorder" Property="Margin" Value="0" />
                            <!--  対応内容：#235 Restoreボタン追加  -->
                            <Setter TargetName="restorButton" Property="Visibility" Value="Visible" />
                            <Setter TargetName="maximizeButton" Property="Visibility" Value="Collapsed" />
                            <!--  RedminePowerカスタマイズ箇所↑↑  -->
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="telerikRibbonView:RadRibbonWindow.IsAutoHideTaskbar" Value="true" />
                                <Condition Property="Window.WindowState" Value="Maximized" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="MaximizeWindowDecorator" Property="Margin" Value="-7,-2,-7,-6" />
                        </MultiTrigger>
                        <Trigger Property="IsTitleVisible" Value="False">
                            <Setter TargetName="WindowTitle" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--  RedminePowerカスタマイズ箇所↓↓  -->
    <!--  Ribbon の上に配置した TextBlock に Disable 時のスタイルが適用されなかったため追加  -->
    <Style x:Key="RibbonTextBlockKStyle"
           BasedOn="{StaticResource {x:Type TextBlock}}"
           TargetType="TextBlock">
        <Setter Property="VerticalAlignment" Value="Center" />
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <!--  RedminePowerカスタマイズ箇所↑↑  -->

    <Style BasedOn="{StaticResource RadRibbonWindowStyle}"
           TargetType="telerikRibbonView:RadRibbonWindow" />
</ResourceDictionary>
