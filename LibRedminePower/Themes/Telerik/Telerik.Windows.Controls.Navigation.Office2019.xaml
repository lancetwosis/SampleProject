    <Style x:Key="LayoutControlDropIndicatorStyle"
           TargetType="layoutControl:LayoutControlDropIndicator">
        <Setter Property="MinWidth" Value="6" />
        <Setter Property="MinHeight" Value="6" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlDropIndicator">
                    <Border x:Name="BorderVisual"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <Border x:Name="OuterBorderVisual"
                                Background="{TemplateBinding Background}"
