                        <Trigger SourceName="SettingsButton" Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                        </Trigger>
                        <Trigger SourceName="SettingsButton" Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                        </Trigger>
                        <Trigger SourceName="SettingsButton" Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledIconBrush}" />
                        </Trigger>
