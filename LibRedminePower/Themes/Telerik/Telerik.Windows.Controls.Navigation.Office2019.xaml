            <Setter.Value>
                <ItemsPanelTemplate>
                    <layoutControl:LayoutControlPanel Orientation="{Binding Orientation, RelativeSource={RelativeSource AncestorType=telerikNavigation:RadLayoutControl}}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadLayoutControl">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <ScrollViewer x:Name="PART_RenderSurface"
                                          Margin="0"
                                          Background="Transparent"
                                          BorderThickness="0"
                                          HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
