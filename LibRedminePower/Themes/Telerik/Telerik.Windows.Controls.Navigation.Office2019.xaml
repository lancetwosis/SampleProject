        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadTileView">
                    <Grid SnapsToDevicePixels="True">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Border Grid.RowSpan="2"
                                Grid.ColumnSpan="2"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}">
                            <ScrollViewer x:Name="ScrollViewer"
                                          Background="{x:Null}"
                                          BorderThickness="0"
                                          CanContentScroll="True"
                                          HorizontalScrollBarVisibility="{Binding (ScrollViewer.HorizontalScrollBarVisibility),
                                                                                  RelativeSource={RelativeSource TemplatedParent}}"
                                          VerticalScrollBarVisibility="{Binding (ScrollViewer.VerticalScrollBarVisibility),
                                                                                RelativeSource={RelativeSource TemplatedParent}}">
                                <ItemsPresenter />
                            </ScrollViewer>
                        </Border>
                        <Border x:Name="MaximizeAreaTop"
                                Grid.Row="0"
                                Grid.ColumnSpan="2"
                                Background="{telerik1:Office2019Resource ResourceKey=AccentBackgroundBrush}"
                                Opacity="0.2"
                                Visibility="Collapsed" />
                        <Border x:Name="MaximizeAreaBottom"
                                Grid.Row="1"
                                Grid.ColumnSpan="2"
                                Background="{telerik1:Office2019Resource ResourceKey=AccentBackgroundBrush}"
                                Opacity="0.2"
                                Visibility="Collapsed" />
                        <Border x:Name="MaximizeAreaLeft"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Background="{telerik1:Office2019Resource ResourceKey=AccentBackgroundBrush}"
                                Opacity="0.2"
                                Visibility="Collapsed" />
                        <Border x:Name="MaximizeAreaRight"
                                Grid.RowSpan="2"
                                Grid.Column="1"
                                Background="{telerik1:Office2019Resource ResourceKey=AccentBackgroundBrush}"
                                Opacity="0.2"
                                Visibility="Collapsed" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="PossibleDockingPosition" Value="Left">
                            <Setter TargetName="MaximizeAreaLeft" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaRight" Property="Visibility" Value="Visible" />
                            <Setter TargetName="MaximizeAreaTop" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaBottom" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="PossibleDockingPosition" Value="Right">
                            <Setter TargetName="MaximizeAreaLeft" Property="Visibility" Value="Visible" />
                            <Setter TargetName="MaximizeAreaRight" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaTop" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaBottom" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="PossibleDockingPosition" Value="Top">
                            <Setter TargetName="MaximizeAreaLeft" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaRight" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaTop" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaBottom" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="PossibleDockingPosition" Value="Bottom">
                            <Setter TargetName="MaximizeAreaLeft" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaRight" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaTop" Property="Visibility" Value="Visible" />
                            <Setter TargetName="MaximizeAreaBottom" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="PossibleDockingPosition" Value="{x:Null}">
                            <Setter TargetName="MaximizeAreaLeft" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaRight" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaTop" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaBottom" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
