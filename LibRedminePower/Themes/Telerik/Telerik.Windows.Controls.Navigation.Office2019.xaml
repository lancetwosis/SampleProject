    <Style x:Key="RadialMenuButtonStyle"
           TargetType="telerikRadialMenu:RadialMenuButton">
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="FontWeight" Value="Normal" />
        <Setter Property="Foreground" Value="{telerik:Office2019Resource ResourceKey=CheckedForegroundBrush}" />
        <Setter Property="Background" Value="{telerik:Office2019Resource ResourceKey=AccentBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik:Office2019Resource ResourceKey=AccentBorderBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="MinWidth" Value="0" />
        <Setter Property="Width" Value="40" />
        <Setter Property="Height" Value="40" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <telerik:RadGlyph FontSize="16"
                                      Glyph="{StaticResource GlyphGear}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="BackContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <telerik:RadGlyph Margin="0,4,0,0"
                                      FontSize="16"
                                      Glyph="{StaticResource GlyphArrowLeft}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRadialMenu:RadialMenuButton">
                    <Grid x:Name="RootGrid">
                        <Grid>
                            <Ellipse x:Name="BackgroundGlyph"
                                     Width="{TemplateBinding Width}"
                                     Height="{TemplateBinding Height}"
                                     Fill="{TemplateBinding Background}"
                                     RenderTransformOrigin=".5 .5"
                                     Stroke="{TemplateBinding BorderBrush}"
                                     StrokeThickness="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                               Path=BorderThickness.Left}" />
                            <ContentPresenter x:Name="NormalGlyph"
                                              Margin="0,-1,0,0"
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Center"
                                              Content="{TemplateBinding Content}"
                                              ContentTemplate="{TemplateBinding ContentTemplate}"
                                              RenderTransformOrigin="0.5 0.5" />
                            <ContentPresenter x:Name="BackGlyph"
                                              Margin="0,-3,0,0"
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Center"
                                              ContentTemplate="{Binding Path=BackContentTemplate,
                                                                        RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                                              RenderTransformOrigin="0.5 0.5"
                                              Visibility="Collapsed" />
                        </Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="NavigationStates">
                                <VisualState x:Name="Back">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NormalGlyph"
                                                                       Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BackGlyph"
                                                                       Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Home">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NormalGlyph"
                                                                       Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BackGlyph"
                                                                       Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                            <Setter TargetName="BackgroundGlyph" Property="Fill" Value="{telerik:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
                            <Setter TargetName="BackgroundGlyph" Property="Stroke" Value="{telerik:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                            <Setter TargetName="BackgroundGlyph" Property="Fill" Value="{telerik:Office2019Resource ResourceKey=AccentPressedBackgroundBrush}" />
                            <Setter TargetName="BackgroundGlyph" Property="Stroke" Value="{x:Null}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="NormalGlyph" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="BackGlyph" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledCheckedForegroundBrush}" />
                        </Trigger>
                        <Trigger Property="FlowDirection" Value="RightToLeft">
                            <Setter Property="BackContentTemplate">
                                <Setter.Value>
                                    <DataTemplate>
                                        <telerik:RadGlyph Margin="0,4,0,0"
                                                          FontSize="16"
                                                          Glyph="{StaticResource GlyphArrowRight}" />
                                    </DataTemplate>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="NavigationItemButtonStyle"
           TargetType="telerikRadialMenu:NavigationItemButton">
        <Setter Property="Foreground" Value="{telerik:Office2019Resource ResourceKey=CheckedForegroundBrush}" />
        <Setter Property="Background" Value="{telerik:Office2019Resource ResourceKey=AccentBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRadialMenu:NavigationItemButton">
