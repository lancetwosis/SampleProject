    <!--  RedminePowerカスタマイズ箇所↑↑  -->
    <ControlTemplate x:Key="RadWindowTemplate"
                     TargetType="telerikNavigation:RadWindow">
        <Grid x:Name="LayoutRoot">
            <!--  RedminePowerカスタマイズ箇所↓↓  -->
            <!--  対応内容：#165 Window枠に影をつける  -->
            <!--<mat:Shadow x:Name="Shadow"
                        Margin="0,3,0,0"
                        Background="{TemplateBinding Background}"
                        CornerRadius="{TemplateBinding CornerRadius}"
                        ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />-->
            <Border x:Name="ShadowBorder"
                    Margin="7"
                    Background="Transparent"
                    BorderThickness="0"
                    ClipToBounds="False">
