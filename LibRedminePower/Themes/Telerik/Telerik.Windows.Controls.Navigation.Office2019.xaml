    <Style x:Key="LayoutControlStructureTabTreeViewItem"
           BasedOn="{StaticResource RadTreeViewItemStyle}"
           TargetType="telerikNavigation:RadTreeViewItem">
        <Setter Property="IsExpanded" Value="True" />
        <Setter Property="Background" Value="#01FFFFFF" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="Padding" Value="0" />
    </Style>
    <HierarchicalDataTemplate x:Key="StructureItemHierarchicalDataTemplate"
                              ItemsSource="{Binding Items}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Path VerticalAlignment="Top"
                  Data="{Binding OriginalItemType,
                                 Converter={StaticResource TypeToPathDataConverter}}"
                  Fill="{Binding Foreground,
                                 ElementName=HeaderText}" />
            <TextBlock x:Name="HeaderText"
                       Grid.Column="1"
                       Margin="10,0,0,0"
                       VerticalAlignment="Center"
                       Text="{Binding Header}" />
        </Grid>
    </HierarchicalDataTemplate>
    <Style x:Key="LayoutControlToolBoxViewStyle"
           TargetType="layoutControl:LayoutControlToolBoxView">
        <Setter Property="telerik:DragDropManager.AllowDrag" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlToolBoxView">
                    <Grid>
                        <telerikNavigation:LayoutControlTabGroup Grid.Row="1"
                                                                 AutomationProperties.Name="LayoutControlToolBoxView LayoutControlTabGroup">
                            <telerikNavigation:LayoutControlTabGroupItem Header="Items">
                                <StackPanel Margin="5"
                                            Orientation="Vertical">
                                    <Grid>
                                        <telerik:RadExpander Padding="0"
                                                             Header="New Items"
                                                             HeaderButtonStyle="{StaticResource LayoutControlToolBoxExpanderHeaderButton}"
                                                             IsExpanded="True">
                                            <telerik:RadListBox Padding="0"
                                                                Background="{x:Null}"
                                                                BorderThickness="1,0,1,1"
                                                                ItemTemplate="{StaticResource StructureItemHierarchicalDataTemplate}"
                                                                ItemsSource="{Binding NewItems,
                                                                                      RelativeSource={RelativeSource TemplatedParent}}"
                                                                ScrollViewer.VerticalScrollBarVisibility="Disabled" />
                                        </telerik:RadExpander>
                                    </Grid>
                                    <Grid>
                                        <telerik:RadExpander Padding="0"
                                                             Header="Deleted Items"
                                                             HeaderButtonStyle="{StaticResource LayoutControlToolBoxExpanderHeaderButton}"
                                                             IsEnabled="{Binding ElementName=deletedItemsList,
                                                                                 Path=HasItems}"
                                                             IsExpanded="True"
                                                             Visibility="{TemplateBinding TrackDeletedItems,
                                                                                          Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <telerik:RadListBox x:Name="deletedItemsList"
                                                                Padding="0"
                                                                Background="{x:Null}"
                                                                BorderThickness="1,0,1,1"
                                                                ItemTemplate="{StaticResource StructureItemHierarchicalDataTemplate}"
                                                                ItemsSource="{Binding DeletedItems,
                                                                                      RelativeSource={RelativeSource TemplatedParent}}"
                                                                ScrollViewer.VerticalScrollBarVisibility="Disabled"
                                                                Visibility="{Binding HasItems,
                                                                                     RelativeSource={RelativeSource Self},
                                                                                     Converter={StaticResource BooleanToVisibilityConverter}}" />
                                        </telerik:RadExpander>
                                    </Grid>
                                </StackPanel>
                            </telerikNavigation:LayoutControlTabGroupItem>
                            <telerikNavigation:LayoutControlTabGroupItem Header="Structure">
                                <telerikNavigation:RadTreeView x:Name="PART_StructureTree"
                                                               ItemContainerStyle="{StaticResource LayoutControlStructureTabTreeViewItem}"
                                                               ItemTemplate="{StaticResource StructureItemHierarchicalDataTemplate}" />
                            </telerikNavigation:LayoutControlTabGroupItem>
                        </telerikNavigation:LayoutControlTabGroup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlResizerStyle"
           TargetType="{x:Type Thumb}">
        <Setter Property="Background" Value="#01FFFFFF" />
        <Setter Property="MinHeight" Value="11" />
        <Setter Property="MinWidth" Value="11" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Thumb}">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlSelectionIndicatorStyle"
           TargetType="layoutControl:LayoutControlSelectionIndicator">
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=AccentBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlSelectionIndicator">
                    <Grid x:Name="GridRoot">
                        <Border BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                            <Grid>
                                <Border Background="{TemplateBinding Background}"
                                        Opacity="0.2" />
                                <Grid x:Name="LockPathGrid"
                                      Width="48"
                                      Height="48"
                                      Opacity="0">
                                    <Ellipse Stretch="UniformToFill"
                                             Stroke="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}"
                                             StrokeThickness="2" />
                                    <telerik:RadGlyph HorizontalAlignment="Center"
                                                      VerticalAlignment="Center"
                                                      FontSize="24"
                                                      Foreground="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}"
                                                      Glyph="{StaticResource GlyphLock}" />
                                </Grid>
                            </Grid>
                        </Border>
                        <Thumb x:Name="PART_TopResizer"
                               Margin="0,-5,0,0"
                               HorizontalAlignment="Stretch"
                               VerticalAlignment="Top"
                               Cursor="SizeNS"
                               Style="{StaticResource LayoutControlResizerStyle}" />
                        <Thumb x:Name="PART_LeftResizer"
                               Margin="-5,0,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Stretch"
                               Cursor="SizeWE"
                               Style="{StaticResource LayoutControlResizerStyle}" />
                        <Thumb x:Name="PART_RightResizer"
                               Margin="0,0,-5,0"
                               HorizontalAlignment="Right"
                               VerticalAlignment="Stretch"
                               Cursor="SizeWE"
                               Style="{StaticResource LayoutControlResizerStyle}" />
                        <Thumb x:Name="PART_BottomResizer"
                               Margin="0,0,0,-5"
                               HorizontalAlignment="Stretch"
                               VerticalAlignment="Bottom"
                               Cursor="SizeNS"
                               Style="{StaticResource LayoutControlResizerStyle}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsContextItemLocked" Value="True">
                            <Setter TargetName="LockPathGrid" Property="Opacity" Value="1" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlMouseOverIndicatorStyle"
           TargetType="layoutControl:LayoutControlMouseOverIndicator">
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border Background="{TemplateBinding Background}"
