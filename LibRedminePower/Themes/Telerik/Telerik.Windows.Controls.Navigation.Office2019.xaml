                                                     Visibility="{TemplateBinding DropDownIndicatorVisibility}">
                                <telerik:RadGlyph x:Name="DropDownIndicator"
                                                  HorizontalAlignment="Center"
                                                  VerticalAlignment="Center"
                                                  FontSize="16"
                                                  Glyph="{StaticResource GlyphArrowOverflowDown}"
                                                  IsHitTestVisible="False" />
                            </telerik:RadToggleButton>
                        </Grid>
                        <Popup x:Name="DropDownPopup"
                               AllowsTransparency="True"
                               Focusable="False"
                               IsOpen="{Binding Path=IsOpen,
                                                Mode=TwoWay,
                                                RelativeSource={RelativeSource TemplatedParent}}"
                               Placement="Bottom"
                               StaysOpen="False"
                               VerticalOffset="-1">
                            <Grid>
                                <Border x:Name="DropDownPopupBorder"
                                        Width="{TemplateBinding DropDownWidth}"
                                        Height="{TemplateBinding DropDownHeight}"
                                        MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                        MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                        Background="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}"
                                        BorderBrush="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}"
                                        BorderThickness="1">
                                    <ContentPresenter x:Name="DropDownPopupContent"
                                                      HorizontalAlignment="Stretch"
                                                      VerticalAlignment="Stretch"
                                                      Content="{TemplateBinding DropDownContent}"
                                                      ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                                      DataContext="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                            Path=DataContext}" />
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="BarItemControlTemplate"
                     TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <Border x:Name="headerContainer"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}">
                <telerik:RadSplitButton x:Name="SplitButton"
                                        Command="{x:Static telerikBreadcrumb:RadBreadcrumbCommands.ClearSelectedItem}"
                                        Content="{TemplateBinding Header}"
                                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                        FontSize="{TemplateBinding FontSize}"
                                        Foreground="{Binding Foreground,
                                                             RelativeSource={RelativeSource TemplatedParent}}"
                                        IsOpen="{Binding Path=IsPopupOpen,
                                                         Mode=TwoWay,
                                                         RelativeSource={RelativeSource TemplatedParent}}"
                                        Style="{TemplateBinding SplitButtonStyle}">
                    <telerik:RadSplitButton.DropDownContent>
                        <ScrollViewer MinWidth="148"
                                      MaxWidth="270"
                                      MaxHeight="250"
                                      Margin="0"
                                      Padding="0,5"
                                      BorderThickness="0"
                                      HorizontalScrollBarVisibility="Auto"
                                      VerticalScrollBarVisibility="Auto">
                            <ItemsPresenter />
                        </ScrollViewer>
                    </telerik:RadSplitButton.DropDownContent>
                </telerik:RadSplitButton>
            </Border>
            <Border x:Name="FocusVisual"
                    Background="{x:Null}"
                    BorderBrush="{TemplateBinding helpers:ThemeHelper.FocusBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    IsHitTestVisible="False"
                    Visibility="Collapsed" />
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="SplitButtonVisibility">
                    <VisualState x:Name="SplitButtonVisible" />
                    <VisualState x:Name="SplitButtonHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="SplitButton"
                                                           Storyboard.TargetProperty="Visibility"
                                                           Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="HasItems" Value="False">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="HasItems" Value="True">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsLinearMode" Value="True">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="IsButtonPartHidden" Value="True">
                <Setter TargetName="SplitButton" Property="IsButtonPartVisible" Value="False" />
            </Trigger>
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="DropDownItemControlTemplate"
                     TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid MinHeight="22">
            <Border x:Name="BorderVisual"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}" />
            <Grid Background="Transparent">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="22" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Image Source="{Binding RelativeSource={RelativeSource TemplatedParent},
                                        Path=Image}"
                       Style="{StaticResource BreadcrumbImageStyle}"
                       Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                            Path=IsIconVisible,
                                            Converter={StaticResource BooleanToVisibilityConverter}}" />
                <ContentControl x:Name="contentControl"
                                Grid.Column="1"
                                Margin="{TemplateBinding Padding}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                Content="{TemplateBinding DropDownHeader}"
                                ContentTemplate="{TemplateBinding DropDownHeaderTemplate}"
                                ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                FontSize="{TemplateBinding FontSize}"
                                Foreground="{TemplateBinding Foreground}" />
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="ShouldHighlight" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=CheckedForegroundBrush}" />
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2019Resource ResourceKey=AccentBackgroundBrush}" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="ShouldHighlight" Value="True" />
                    <Condition Property="IsSelected" Value="True" />
                </MultiTrigger.Conditions>
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadBreadcrumbStyle"
           TargetType="telerikNavigation:RadBreadcrumb">
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}" />
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik1:Office2019Resource ResourceKey=DisabledBorderBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="MinWidth" Value="150" />
        <Setter Property="MinHeight" Value="22" />
        <Setter Property="SplitButtonStyle" Value="{StaticResource BreadcrumbSplitButtonStyle}" />
        <Setter Property="OverflowButtonStyle" Value="{StaticResource BreadcrumbOverflowSplitButtonStyle}" />
        <Setter Property="DropDownItemControlTemplate" Value="{StaticResource DropDownItemControlTemplate}" />
        <Setter Property="BarItemControlTemplate" Value="{StaticResource BarItemControlTemplate}" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadBreadcrumb">
                    <Grid x:Name="outerGrid">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="22" />
                        </Grid.ColumnDefinitions>
                        <Border x:Name="NormalVisual"
                                Grid.ColumnSpan="3"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        <Image x:Name="image"
                               Margin="6,0"
                               Source="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                Path=CurrentContainer.Image}"
                               Style="{StaticResource BreadcrumbImageStyle}"
                               Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                    Path=IsCurrentIconVisible,
                                                    Converter={StaticResource BooleanToVisibilityConverter}}" />
                        <telerik:RadSplitButton x:Name="historySplitButton"
                                                Grid.Column="1"
                                                Grid.ColumnSpan="2"
                                                MinHeight="{TemplateBinding MinHeight}"
                                                BorderThickness="{TemplateBinding BorderThickness}"
                                                IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                 Path=IsHistoryOpen,
                                                                 Mode=TwoWay}"
                                                IsTabStop="False"
                                                Style="{StaticResource BreadcrumbTextSplitButton}"
                                                TogglePartStyle="{StaticResource BreadCrumbHistoryToggleButtonStyle}">
                            <telerik:RadSplitButton.Content>
                                <TextBox x:Name="pathTextBox"
                                         VerticalAlignment="Stretch"
                                         helpers:ThemeHelper.FocusBrush="{x:Null}"
                                         helpers:ThemeHelper.MouseOverBrush="{x:Null}"
                                         AutomationProperties.Name="history"
                                         Background="{x:Null}"
                                         BorderBrush="{x:Null}"
                                         BorderThickness="0"
                                         FontSize="{TemplateBinding FontSize}"
                                         IsTabStop="False"
                                         Opacity="0" />
                            </telerik:RadSplitButton.Content>
                            <telerik:RadSplitButton.DropDownContent>
                                <ListBox x:Name="HistoryListBox"
                                         HorizontalContentAlignment="Stretch"
                                         VerticalContentAlignment="Stretch"
                                         BorderThickness="0"
                                         FontSize="{TemplateBinding FontSize}"
                                         Foreground="{TemplateBinding Foreground}"
                                         ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                         ItemsSource="{TemplateBinding HistoryItems}"
                                         SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                Path=SelectedHistoryItem,
                                                                Mode=TwoWay}" />
                            </telerik:RadSplitButton.DropDownContent>
                        </telerik:RadSplitButton>
                        <telerikBreadcrumb:RadBreadcrumbBar x:Name="BreadcrumbBar"
                                                            Grid.Column="1"
                                                            Background="Transparent"
                                                            FontSize="{TemplateBinding FontSize}"
                                                            Foreground="{TemplateBinding Foreground}"
                                                            ItemTemplate="{TemplateBinding ItemTemplate}"
                                                            ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}" />
                        <Border x:Name="FocusVisual"
                                Grid.ColumnSpan="3"
                                Background="{x:Null}"
                                BorderBrush="{TemplateBinding helpers:ThemeHelper.FocusBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                IsHitTestVisible="False"
                                Visibility="Collapsed" />
                        <Popup x:Name="AutocompletePopup"
                               AllowsTransparency="True"
                               Focusable="False"
                               Placement="Bottom"
                               StaysOpen="False"
                               VerticalOffset="-1">
                            <Grid>
                                <ListBox x:Name="AutoCompleteListBox"
                                         MinWidth="150"
                                         MaxWidth="650"
                                         MaxHeight="500"
                                         FontSize="{TemplateBinding FontSize}"
                                         Foreground="{TemplateBinding Foreground}"
                                         ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                         ItemsSource="{TemplateBinding AutocompleteItems}"
                                         SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                Path=SelectedAutocompleteItem,
                                                                Mode=TwoWay}" />
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsTextModeEnabled" Value="False">
                            <Setter TargetName="historySplitButton" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True" />
                                <Condition Property="IsTextMode" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
                            <Setter TargetName="pathTextBox" Property="Opacity" Value="1" />
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True" />
                                <Condition Property="IsTextMode" Value="False" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Visible" />
                        </MultiTrigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="NormalVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
