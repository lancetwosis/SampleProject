    <ControlTemplate x:Key="LayoutControlExpanderGroupControlTemplate"
                     TargetType="telerikNavigation:LayoutControlExpanderGroup">
        <Grid>
            <Border Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}" />
            <telerik:RadExpander Padding="{TemplateBinding Padding}"
                                 animation:AnimationManager.IsAnimationEnabled="{Binding Path=(animation:AnimationManager.IsAnimationEnabled),
                                                                                         RelativeSource={RelativeSource TemplatedParent}}"
                                 Background="{x:Null}"
                                 BorderBrush="{x:Null}"
                                 BorderThickness="0"
                                 Header="{TemplateBinding Header}"
                                 HeaderButtonStyle="{StaticResource LayoutControlExpanderHeaderButtonStyle}"
                                 IsExpanded="{Binding IsExpanded,
                                                      RelativeSource={RelativeSource TemplatedParent},
                                                      Mode=TwoWay}">
                <ScrollViewer Background="Transparent"
                              BorderThickness="0"
                              HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
                              VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}">
                    <ItemsPresenter />
                </ScrollViewer>
            </telerik:RadExpander>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="LayoutControlExpanderGroupStyle"
           BasedOn="{StaticResource LayoutControlGroupBaseStyle}"
           TargetType="telerikNavigation:LayoutControlExpanderGroup">
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik1:Office2019Resource ResourceKey=DisabledBackgroundBrush}" />
        <Setter Property="animation:AnimationManager.IsAnimationEnabled" Value="False" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <layoutControl:LayoutControlPanel Orientation="{Binding Orientation, RelativeSource={RelativeSource AncestorType=telerikNavigation:LayoutControlExpanderGroup}}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource LayoutControlExpanderGroupControlTemplate}" />
    </Style>
    <ControlTemplate x:Key="LayoutControlTabGroupControlTemplate"
                     TargetType="telerikNavigation:LayoutControlTabGroup">
        <Grid Background="#01FFFFFF">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <Border x:Name="TabStripBackground"
                    Background="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{telerik1:Office2019Resource ResourceKey=HeaderBorderThickness}">
                <ItemsPresenter />
