    </Style>
    <Style x:Key="ToolBarOverflowButtonStyle"
           BasedOn="{StaticResource RadToggleButtonStyle}"
           TargetType="telerik:RadToggleButton">
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=IconBrush}" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="Margin" Value="2" />
        <Setter Property="MinWidth" Value="22" />
        <Setter Property="Focusable" Value="False" />
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledIconBrush}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="RadToolBarStyle"
           TargetType="telerikNavigation:RadToolBar">
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik1:Office2019Resource ResourceKey=DisabledBorderBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="MinWidth" Value="26" />
        <Setter Property="Padding" Value="0,1" />
        <Setter Property="UseLayoutRounding" Value="True" />
        <Setter Property="FocusManager.IsFocusScope" Value="True" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="ItemContainerStyleSelector">
            <Setter.Value>
                <telerikNavigation:ToolBarContainerStyleSelector>
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource RadToolBarSeparatorStyle}"
                                                             TypeName="RadToolBarSeparator" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarTextBlockStyle}"
                                                             TypeName="TextBlock" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarTextBoxStyle}"
                                                             TypeName="TextBox" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarButtonStyle}"
                                                             TypeName="Button" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRepeatButtonStyle}"
                                                             TypeName="RepeatButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarToggleButtonStyle}"
                                                             TypeName="ToggleButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarCheckBoxStyle}"
                                                             TypeName="CheckBox" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadioButtonStyle}"
                                                             TypeName="RadioButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadButtonStyle}"
                                                             TypeName="RadButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadPathButtonStyle}"
                                                             TypeName="RadPathButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadToggleButtonStyle}"
                                                             TypeName="RadToggleButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadRadioButtonStyle}"
                                                             TypeName="RadRadioButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadDropDownButtonStyle}"
                                                             TypeName="RadDropDownButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadSplitButtonStyle}"
                                                             TypeName="RadSplitButton" />
                </telerikNavigation:ToolBarContainerStyleSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadToolBar">
                    <Grid x:Name="RootElement"
                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Name="c0"
                                              Width="Auto" />
                            <ColumnDefinition x:Name="c1"
                                              Width="*" />
                            <ColumnDefinition x:Name="c2"
                                              Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="r0"
                                           Height="*" />
                            <RowDefinition x:Name="r1"
                                           Height="Auto" />
                            <RowDefinition x:Name="r2"
                                           Height="Auto" />
                        </Grid.RowDefinitions>
                        <Border x:Name="BorderVisual"
                                Grid.RowSpan="3"
                                Grid.ColumnSpan="3"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}" />
                        <telerik:RadToggleButton x:Name="PART_DropdownButton"
                                                 Grid.Row="0"
                                                 Grid.RowSpan="2"
                                                 Grid.Column="2"
                                                 IsChecked="{TemplateBinding IsOverflowOpen}"
                                                 IsEnabled="{TemplateBinding HasOverflowItems}"
                                                 Style="{StaticResource ToolBarOverflowButtonStyle}"
                                                 Visibility="{TemplateBinding OverflowButtonVisibility}">
                            <telerik:RadGlyph x:Name="arrow"
                                              FontSize="16"
                                              Glyph="{StaticResource GlyphArrowOverflowDown}" />
                        </telerik:RadToggleButton>
                        <Grid x:Name="PopupGrid"
                              Grid.Column="2">
                            <Popup x:Name="PART_Overflow"
                                   AllowsTransparency="True"
                                   Focusable="false"
                                   HorizontalOffset="0"
                                   IsOpen="{Binding Path=IsOverflowOpen,
                                                    RelativeSource={RelativeSource TemplatedParent}}"
                                   Placement="Bottom"
                                   PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}"
                                   StaysOpen="False"
                                   VerticalOffset="-1">
                                <Grid>
                                    <Border Background="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}"
                                            BorderBrush="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}"
                                            BorderThickness="1">
                                        <telerikToolBar:RadToolBarOverflowPanel x:Name="PART_OverflowPanel"
                                                                                Margin="0,5"
                                                                                VerticalAlignment="Top"
                                                                                Orientation="Vertical" />
                                    </Border>
                                </Grid>
                            </Popup>
                        </Grid>
                        <Grid Background="Transparent"
                              Visibility="{TemplateBinding GripVisibility}">
                            <telerik:RadGlyph x:Name="GripOrnamentH"
                                              FontSize="16"
                                              Foreground="{telerik1:Office2019Resource ResourceKey=IconBrush}"
                                              Glyph="{StaticResource GlyphMoreVertical}"
                                              Visibility="Visible" />
                            <telerik:RadGlyph x:Name="GripOrnamentV"
                                              FontSize="16"
                                              Foreground="{telerik1:Office2019Resource ResourceKey=IconBrush}"
                                              Glyph="{StaticResource GlyphMoreHorizontal}"
                                              Visibility="Collapsed" />
                        </Grid>
                        <telerikToolBar:RadToolBarPanel x:Name="PART_StripPanel"
                                                        Grid.Row="0"
                                                        Grid.Column="1"
                                                        Margin="{TemplateBinding Padding}"
                                                        Orientation="Horizontal" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Orientation" Value="Horizontal" />
                        <Trigger Property="Orientation" Value="Vertical">
                            <Setter TargetName="c0" Property="Width" Value="*" />
                            <Setter TargetName="c1" Property="Width" Value="Auto" />
                            <Setter TargetName="r0" Property="Height" Value="Auto" />
                            <Setter TargetName="r1" Property="Height" Value="*" />
                            <Setter TargetName="PART_StripPanel" Property="Orientation" Value="Vertical" />
                            <Setter TargetName="PART_StripPanel" Property="Grid.Row" Value="1" />
                            <Setter TargetName="PART_StripPanel" Property="Grid.Column" Value="0" />
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Row" Value="2" />
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Column" Value="0" />
                            <Setter TargetName="GripOrnamentH" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="GripOrnamentV" Property="Visibility" Value="Visible" />
                            <Setter TargetName="PopupGrid" Property="Grid.Row" Value="2" />
                            <Setter TargetName="PopupGrid" Property="Grid.Column" Value="0" />
                            <Setter TargetName="PART_Overflow" Property="Placement" Value="Right" />
                            <Setter TargetName="PART_Overflow" Property="HorizontalOffset" Value="-1" />
                            <Setter TargetName="PART_Overflow" Property="VerticalOffset" Value="0" />
                            <Setter TargetName="PART_DropdownButton" Property="MinHeight" Value="22" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="GripOrnamentH" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="GripOrnamentV" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="arrow" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}" />
                            <Setter TargetName="GripOrnamentH" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledIconBrush}" />
                            <Setter TargetName="GripOrnamentV" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledIconBrush}" />
                            <Setter TargetName="arrow" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledIconBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadToolBarTrayStyle"
           TargetType="telerikNavigation:RadToolBarTray">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Padding" Value="1,0" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikToolBar:RadToolBarTrayPanel x:Name="PART_BandPanel" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadToolBarTray">
                    <Border Background="{TemplateBinding Background}"
