    <Style x:Key="BreadcrumbTextSplitButton"
           BasedOn="{StaticResource RadSplitButtonStyle}"
           TargetType="telerik:RadSplitButton">
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbHistoryToggleButtonStyle}" />
        <Setter Property="Background" Value="{x:Null}" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSplitButton">
