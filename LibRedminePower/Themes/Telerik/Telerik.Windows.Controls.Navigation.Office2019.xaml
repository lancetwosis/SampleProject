    <Style x:Key="RadOutlookBarStyle"
           TargetType="telerikNavigation:RadOutlookBar">
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik1:Office2019Resource ResourceKey=DisabledBorderBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HeaderBackground" Value="{telerik1:Office2019Resource ResourceKey=HeaderBackgroundBrush}" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="MinimizedAreaMinHeight" Value="26" />
        <Setter Property="MinimizedWidth" Value="26" />
        <Setter Property="MinWidth" Value="26" />
        <Setter Property="MinimizeButtonStyle" Value="{StaticResource OutlookBarMinimizeButtonStyle}" />
        <Setter Property="MinimizedButtonContentTemplate" Value="{StaticResource OutlookBarMinimizedButtonContentTemplate}" />
        <Setter Property="DropDownDisplayMode" Value="Visible" />
        <Setter Property="DropDownStyle" Value="{StaticResource OutlookBarDropDownStyle}" />
        <Setter Property="HorizontalSplitterStyle" Value="{StaticResource OutlookBarHorizontalSplitterStyle}" />
        <Setter Property="VerticalResizerStyle" Value="{StaticResource OutlookBarVerticalResizerStyle}" />
        <Setter Property="KeyboardNavigation.TabNavigation" Value="Once" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <outlookBarPrimitives:OutlookBarPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadOutlookBar">
                    <Grid x:Name="LayoutRoot">
                        <Border x:Name="RootElement"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}">
                            <DockPanel>
                                <Border x:Name="HeaderBorder"
                                        Background="{TemplateBinding HeaderBackground}"
                                        BorderBrush="{TemplateBinding HeaderBackground}"
                                        BorderThickness="{TemplateBinding BorderThickness}"
                                        DockPanel.Dock="Top"
                                        Visibility="{TemplateBinding HeaderVisibility}">
                                    <Grid MinHeight="26">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <ContentControl x:Name="TitleElement"
                                                        VerticalAlignment="Center"
                                                        HorizontalContentAlignment="Stretch"
                                                        VerticalContentAlignment="Center"
                                                        ContentControl.Foreground="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}"
                                                        ContentControl.Margin="5 0 0 0"
                                                        ContentTemplate="{TemplateBinding TitleTemplate}"
                                                        FontSize="{telerik1:Office2019Resource ResourceKey=FontSize}"
                                                        IsTabStop="False" />
                                        <telerik1:RadToggleButton x:Name="MinimizeButton"
                                                                  Grid.Column="1"
                                                                  AutomationProperties.Name="Minimize"
                                                                  IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                      Path=IsMinimized,
                                                                                      Mode=TwoWay}"
                                                                  Style="{TemplateBinding MinimizeButtonStyle}"
                                                                  Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                       Path=IsMinimizable,
                                                                                       Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <telerik1:RadGlyph x:Name="arrowGlyph"
                                                               FontSize="16"
                                                               Foreground="{Binding RelativeSource={RelativeSource AncestorType=telerik1:RadToggleButton},
                                                                                    Path=Foreground}"
                                                               Glyph="{StaticResource GlyphArrowChevronLeft}" />
                                        </telerik1:RadToggleButton>
                                    </Grid>
                                </Border>
                                <Border x:Name="MinimizedAreaBorder"
                                        MinHeight="{TemplateBinding MinimizedAreaMinHeight}"
                                        Background="{TemplateBinding Background}"
                                        BorderBrush="{TemplateBinding BorderBrush}"
                                        BorderThickness="0,1,0,0"
                                        DockPanel.Dock="Bottom"
                                        Visibility="{TemplateBinding MinimizedAreaVisibility}">
                                    <DockPanel>
                                        <ToggleButton x:Name="DropDownButtonElement"
                                                      AutomationProperties.Name="Overflow"
                                                      IsChecked="{Binding IsDropDownOpen,
                                                                          Mode=TwoWay,
                                                                          RelativeSource={RelativeSource TemplatedParent}}"
                                                      Style="{StaticResource OutlookBarDropDownButtonStyle}">
                                            <ToolTipService.ToolTip>
                                                <TextBlock Text="{telerik1:LocalizableResource Key=OutlookBarConfigureButtons}" />
                                            </ToolTipService.ToolTip>
                                            <telerikNavigation:RadContextMenu.ContextMenu>
                                                <telerikTabControl:DropDownMenu x:Name="DropDownMenuElement"
                                                                                DisplayMemberPath="{TemplateBinding DropDownDisplayMemberPath}"
                                                                                ItemTemplate="{TemplateBinding ItemDropDownContentTemplate}"
                                                                                ItemTemplateSelector="{TemplateBinding ItemDropDownContentTemplateSelector}"
                                                                                Placement="Bottom"
                                                                                StaysOpen="False"
                                                                                Style="{TemplateBinding DropDownStyle}" />
                                            </telerikNavigation:RadContextMenu.ContextMenu>
                                        </ToggleButton>
                                        <outlookBarPrimitives:MinimizedOutlookBarArea x:Name="MinimizedAreaControl" />
                                    </DockPanel>
                                </Border>
                                <ItemsPresenter x:Name="ItemsPresenterElement"
                                                DockPanel.Dock="Bottom" />
                                <Thumb x:Name="HorizontalSplitter"
                                       DockPanel.Dock="Bottom"
                                       Style="{TemplateBinding HorizontalSplitterStyle}">
                                    <ToolTipService.ToolTip>
                                        <TextBlock Text="{telerik1:LocalizableResource Key=OutlookBarHorizontalSplitter}" />
                                    </ToolTipService.ToolTip>
                                </Thumb>
                                <Grid>
                                    <ContentPresenter x:Name="ContentElement"
                                                      Margin="{TemplateBinding Padding}"
                                                      HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                      ContentTemplate="{TemplateBinding SelectedContentTemplate}"
                                                      ContentTemplateSelector="{TemplateBinding SelectedContentTemplateSelector}"
                                                      Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                           Path=IsContentPreserved,
                                                                           Converter={StaticResource InvertedBooleanToVisibilityConverter}}" />
                                    <Grid x:Name="ContentElementsPanel"
                                          Margin="{TemplateBinding Padding}"
                                          Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                               Path=IsContentPreserved,
                                                               Converter={StaticResource BooleanToVisibilityConverter}}" />
                                    <telerik1:RadDropDownButton x:Name="MinimizedContentElement"
                                                                Background="{TemplateBinding Background}"
                                                                Content="{TemplateBinding MinimizedButtonContent}"
                                                                ContentTemplate="{TemplateBinding MinimizedButtonContentTemplate}"
                                                                Style="{StaticResource OutlookBarMinimizedContentButtonStyle}"
                                                                Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                     Path=IsMinimized,
                                                                                     Converter={StaticResource BooleanToVisibilityConverter}}">
                                        <telerik1:RadDropDownButton.DropDownContent>
                                            <Grid>
                                                <ContentPresenter x:Name="MinimizedContentPresenter"
                                                                  ContentTemplate="{TemplateBinding SelectedContentTemplate}"
                                                                  ContentTemplateSelector="{TemplateBinding SelectedContentTemplateSelector}" />
                                                <Thumb x:Name="PopupResizer"
                                                       Width="2"
                                                       HorizontalAlignment="Right"
                                                       VerticalAlignment="Stretch"
                                                       Cursor="SizeWE"
                                                       Opacity="0" />
                                            </Grid>
                                        </telerik1:RadDropDownButton.DropDownContent>
                                    </telerik1:RadDropDownButton>
                                </Grid>
                            </DockPanel>
                        </Border>
                        <Thumb x:Name="VerticalResizer"
                               Style="{TemplateBinding VerticalResizerStyle}">
                            <ToolTipService.ToolTip>
                                <TextBlock Text="{telerik1:LocalizableResource Key=OutlookBarVerticalResizer}" />
                            </ToolTipService.ToolTip>
                        </Thumb>
                    </Grid>
                    <ControlTemplate.Triggers>

                        <!--  Minimized  -->
                        <Trigger Property="IsMinimized" Value="True">
                            <Setter TargetName="TitleElement" Property="Opacity" Value="0" />
                            <Setter TargetName="ContentElement" Property="Opacity" Value="0" />
                            <Setter TargetName="arrowGlyph" Property="Glyph" Value="{StaticResource GlyphArrowChevronRight}" />
                            <Setter TargetName="MinimizeButton" Property="Margin" Value="0" />
                            <Setter TargetName="MinimizeButton" Property="BorderThickness" Value="0" />
                            <Setter TargetName="MinimizeButton" Property="VerticalAlignment" Value="Stretch" />
                            <Setter TargetName="MinimizeButton" Property="Width" Value="26" />
                            <Setter TargetName="DropDownButtonElement" Property="Margin" Value="0" />
                            <Setter TargetName="DropDownButtonElement" Property="BorderThickness" Value="0" />
                            <Setter TargetName="DropDownButtonElement" Property="VerticalAlignment" Value="Stretch" />
                            <Setter TargetName="DropDownButtonElement" Property="Width" Value="26" />
                        </Trigger>

                        <!--  Restored  -->
                        <Trigger Property="IsMinimized" Value="False">
                            <Setter TargetName="TitleElement" Property="Opacity" Value="1" />
                            <Setter TargetName="ContentElement" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="RootElement" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}" />
                            <Setter TargetName="MinimizedAreaBorder" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}" />
                            <Setter TargetName="TitleElement" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="arrowGlyph" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="arrowGlyph" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledCheckedForegroundBrush}" />
                            <Setter TargetName="TitleElement" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledCheckedForegroundBrush}" />
                            <Setter TargetName="DropDownButtonElement" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledForegroundBrush}" />
                        </Trigger>
                        <DataTrigger Binding="{Binding IsChecked,
                                                       ElementName=MinimizeButton}"
                                     Value="True">
                            <Setter TargetName="arrowGlyph" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}" />
                        </DataTrigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsChecked, ElementName=MinimizeButton}" Value="True" />
                                <Condition Binding="{Binding IsMouseOver, ElementName=MinimizeButton}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="arrowGlyph" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                        </MultiDataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadOutlookBarItemStyle"
           TargetType="telerikNavigation:RadOutlookBarItem">
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
        <Setter Property="helpers:ThemeHelper.PressedBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentPressedBackgroundBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
