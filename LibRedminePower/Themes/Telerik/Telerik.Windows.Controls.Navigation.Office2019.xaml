                </Grid>
            </Grid>
            <telerikNavigation:NavigationViewSubItemsHost x:Name="PART_NavigationViewSubItemsHost"
                                                          Grid.Row="1"
                                                          Margin="-1,0"
                                                          Background="Transparent"
                                                          DisplayMemberPath="{TemplateBinding DisplayMemberPath}"
                                                          Focusable="False"
                                                          ItemContainerStyle="{TemplateBinding ItemContainerStyle}"
                                                          ItemTemplate="{TemplateBinding ItemTemplate}"
                                                          ItemsSource="{TemplateBinding ItemsSource}"
                                                          UseLayoutRounding="True"
                                                          Visibility="Collapsed" />
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Self},
                                           Path=IsTopLevel}"
                         Value="False">
                <Setter TargetName="ItemContentGrid" Property="Margin" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CalculatedItemIndentation, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Left}" />
            </DataTrigger>
            <Trigger Property="IsExpanded" Value="True">
                <Setter TargetName="ExpandedIcon" Property="Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IsExpanded" Value="False">
                <Setter TargetName="CollapsedIcon" Property="Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IconVisibility" Value="Collapsed">
                <Setter TargetName="IconColumn" Property="Width" Value="0" />
            </Trigger>
            <Trigger Property="IsHighlighted" Value="True">
                <Setter Property="Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="IconContent" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="ExpandedIcon" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="CollapsedIcon" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_MouseOver}" />
                <Setter TargetName="HighlightedVisual" Property="Opacity" Value="1" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="True" />
                    <Condition Property="IsHighlighted" Value="False" />
                </MultiTrigger.Conditions>
                <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_Selected}" />
                <Setter Property="Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="IconContent" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="ExpandedIcon" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="CollapsedIcon" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="Content" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="1" />
            </MultiTrigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_Selected}" />
                <Setter Property="Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="IconContent" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="ExpandedIcon" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="CollapsedIcon" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="Content" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="PressedVisual" Property="Opacity" Value="1" />
            </Trigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsExpanded}" Value="False" />
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=HasSelectedSubItem}" Value="True" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="{StaticResource NavigationView_NavItemBorderBrush_Selected}" />
                <Setter Property="Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="IconContent" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="ExpandedIcon" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="CollapsedIcon" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="Content" Property="TextElement.Foreground" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="1" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsExpanded}" Value="True" />
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=HasSelectedSubItem}" Value="True" />
                </MultiDataTrigger.Conditions>
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="0" />
            </MultiDataTrigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="ItemContentGrid" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadNavigationViewItemStyle"
           TargetType="telerikNavigation:RadNavigationViewItem">
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
        <Setter Property="helpers:ThemeHelper.CheckedBrush" Value="{telerik:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
        <Setter Property="helpers:ThemeHelper.PressedBrush" Value="{telerik:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
        <Setter Property="Foreground" Value="{telerik:Office2019Resource ResourceKey=CheckedForegroundBrush}" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="MinHeight" Value="32" />
        <Setter Property="Padding" Value="5,0" />
        <Setter Property="Margin" Value="1,0" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="ExpandedIcon" Value="{StaticResource GlyphArrowChevronUp}" />
        <Setter Property="ExpandedIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <telerik1:RadGlyph FontSize="16"
                                       Glyph="{Binding}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="CollapsedIcon" Value="{StaticResource GlyphArrowChevronDown}" />
        <Setter Property="CollapsedIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <telerik1:RadGlyph FontSize="16"
                                       Glyph="{Binding}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource RadNavigationViewItemTemplate}" />
    </Style>
    <Style BasedOn="{StaticResource RadNavigationViewStyle}"
           TargetType="telerikNavigation:RadNavigationView" />
    <Style BasedOn="{StaticResource RadNavigationViewItemStyle}"
           TargetType="telerikNavigation:RadNavigationViewItem" />
    <Style x:Key="AddItemsButtonStyle"
           BasedOn="{StaticResource RadButtonStyle}"
           TargetType="telerik:RadButton">
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}" />
        <Setter Property="IsBackgroundVisible" Value="False" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="MinWidth" Value="26" />
        <Setter Property="MinHeight" Value="26" />
    </Style>
    <Style x:Key="RadTabbedWindowTabItemStyle"
           BasedOn="{StaticResource RadTabItemStyle}"
           TargetType="telerikNavigation:RadTabItem">
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
        <Setter Property="helpers:ThemeHelper.CheckedBrush" Value="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}" />
        <Setter Property="HeaderForeground" Value="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="telerik:WindowHost.HitTestable" Value="True" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="CloseButtonVisibility" Value="Visible" />
        <Setter Property="MinHeight" Value="26" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadTabItem">

                    <!--  x:Name is used in code  -->
                    <Grid x:Name="wrapper">
                        <Border x:Name="BorderVisual"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                          Path=BorderThickness,
                                                          Converter={StaticResource ThicknessToOrientedThicknessConverter},
                                                          ConverterParameter=LeftTopRight}" />
                        <telerikPrimitives:LayoutTransformControl x:Name="OrientationTransform">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <ContentPresenter x:Name="HeaderElement"
                                                  Margin="{TemplateBinding Padding}"
                                                  HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                  VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                  Content="{TemplateBinding Header}"
                                                  ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                                                  ContentTemplate="{TemplateBinding HeaderTemplate}"
                                                  ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                                  RecognizesAccessKey="True"
                                                  SnapsToDevicePixels="True"
                                                  TextBlock.Foreground="{TemplateBinding HeaderForeground}" />
                                <telerik:RadButton x:Name="PinButton"
                                                   Grid.Column="1"
                                                   Margin="0,0,5,0"
                                                   helpers:ThemeHelper.MouseOverBrush="Transparent"
                                                   helpers:ThemeHelper.PressedBrush="Transparent"
                                                   BorderThickness="0"
                                                   Command="telerikTabControl:TabItemCommands.TogglePin"
                                                   IsTabStop="False"
                                                   Style="{StaticResource PinCloseButtonStyle}"
                                                   Visibility="{TemplateBinding PinButtonVisibility}">
                                    <telerik:RadGlyph x:Name="PinGlyph"
                                                      FontSize="16"
                                                      Glyph="{StaticResource GlyphUnpin}"
                                                      IsHitTestVisible="False" />
                                </telerik:RadButton>
                                <telerik:RadButton x:Name="CloseButton"
                                                   Grid.Column="2"
                                                   Margin="0,0,5,0"
                                                   helpers:ThemeHelper.MouseOverBrush="Transparent"
                                                   helpers:ThemeHelper.PressedBrush="Transparent"
                                                   BorderThickness="0"
                                                   Command="telerikTabControl:TabItemCommands.Close"
                                                   IsTabStop="False"
                                                   Style="{StaticResource PinCloseButtonStyle}"
                                                   Visibility="{TemplateBinding CloseButtonVisibility}">
                                    <telerik:RadGlyph x:Name="CloseGlyph"
                                                      FontSize="16"
                                                      Glyph="{StaticResource GlyphClose}"
                                                      IsHitTestVisible="False" />
                                </telerik:RadButton>
                            </Grid>
                        </telerikPrimitives:LayoutTransformControl>
                    </Grid>
                    <ControlTemplate.Triggers>

                        <!--  Selected  -->
                        <Trigger Property="IsSelected" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
                            <Setter Property="HeaderForeground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
                            <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CheckedBrush)}" />
                        </Trigger>
                        <Trigger Property="IsSelected" Value="False">
                            <Setter TargetName="PinButton" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}" />
                            <Setter TargetName="CloseButton" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}" />
                        </Trigger>
                        <Trigger SourceName="wrapper" Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                            <Setter Property="HeaderForeground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                            <Setter TargetName="PinButton" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                            <Setter TargetName="CloseButton" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                            <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition SourceName="wrapper" Property="IsMouseOver" Value="True" />
                                <Condition SourceName="PinButton" Property="IsMouseOver" Value="False" />
                                <Condition SourceName="CloseButton" Property="IsMouseOver" Value="False" />
                            </MultiTrigger.Conditions>
                        </MultiTrigger>
                        <Trigger SourceName="PinButton" Property="IsMouseOver" Value="True">
                            <Setter TargetName="PinButton" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBorderBrush}" />
                        </Trigger>
                        <Trigger SourceName="CloseButton" Property="IsMouseOver" Value="True">
                            <Setter TargetName="CloseButton" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=AccentMouseOverBorderBrush}" />
                        </Trigger>
                        <Trigger SourceName="PinButton" Property="IsPressed" Value="True">
                            <Setter TargetName="PinButton" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}" />
                        </Trigger>
                        <Trigger SourceName="CloseButton" Property="IsPressed" Value="True">
                            <Setter TargetName="CloseButton" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=AccentBorderBrush}" />
                        </Trigger>
                        <Trigger Property="IsPinned" Value="True">
                            <Setter TargetName="PinGlyph" Property="Glyph" Value="{StaticResource GlyphPin}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="HeaderElement" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
                            <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=DisabledForegroundBrush}" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsEnabled" Value="False" />
                                <Condition Property="IsSelected" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter Property="Foreground" Value="{Binding Foreground, RelativeSource={RelativeSource TemplatedParent}}" />
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="TabbedWindowTabControlTemplate"
                     TargetType="tabbedWindow:TabbedWindowTabControl">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"
                               MinHeight="26" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid x:Name="PART_HeaderGrid"
                  Margin="-1"
                  SnapsToDevicePixels="True">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Border x:Name="HeaderBackground"
                        Grid.ColumnSpan="3"
                        Background="{TemplateBinding HeaderBackground}"
                        CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent},
                                               Path=(helpers:ThemeHelper.CornerRadius),
                                               Converter={StaticResource CornerRadiusConverter},
                                               ConverterParameter=TopLeftRight}"
                        Visibility="{TemplateBinding BackgroundVisibility}" />
                <Border x:Name="HeaderBorder"
                        Grid.ColumnSpan="3"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{telerik1:Office2019Resource ResourceKey=HeaderBorderThickness}"
                        CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent},
                                               Path=(helpers:ThemeHelper.CornerRadius),
                                               Converter={StaticResource CornerRadiusConverter},
                                               ConverterParameter=TopLeftRight}" />
                <telerikPrimitives:LayoutTransformControl x:Name="AdditionalContentTransform"
                                                          Grid.Column="2"
                                                          Margin="10,0,0,0"
                                                          VerticalAlignment="Center">
                    <ContentPresenter x:Name="AdditionalContentElement"
                                      telerik:CornerRadiusHelper.ClipRadius="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                      Path=(helpers:ThemeHelper.CornerRadius),
                                                                                      Converter={StaticResource CornerRadiusConverter},
                                                                                      ConverterParameter=TopRight}"
                                      telerik:CornerRadiusHelper.ClipRadiusOffset="{TemplateBinding telerik:CornerRadiusHelper.ClipRadiusOffset}"
                                      Content="{TemplateBinding AdditionalContent}" />
                </telerikPrimitives:LayoutTransformControl>
                <telerikPrimitives:LayoutTransformControl x:Name="AdditionalLeftContentTransform"
                                                          VerticalAlignment="Center">
                    <ContentPresenter x:Name="AdditionalLeftContentElement"
                                      Content="{TemplateBinding AdditionalLeftContent}" />
                </telerikPrimitives:LayoutTransformControl>
                <panels:RadTabbedWindowPanel x:Name="PART_TabbedWindowPanel"
                                             Grid.Column="1"
                                             Margin="2,0"
                                             HorizontalAlignment="Left">
                    <RepeatButton x:Name="LeftScrollButtonElement"
                                  MinWidth="26"
                                  MinHeight="26"
                                  Margin="0,0,5,0"
                                  Padding="2"
                                  helpers:ThemeHelper.DisabledBrush="Transparent"
                                  AutomationProperties.Name="scroll left"
                                  Background="Transparent"
                                  BorderBrush="Transparent"
                                  Foreground="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}">
                        <telerik:RadGlyph FontSize="16"
                                          Glyph="{StaticResource GlyphArrowChevronLeft}" />
                    </RepeatButton>
                    <Border x:Name="PART_LeftSeparator"
                            MinHeight="18"
                            Margin="0,0,3,3"
                            BorderBrush="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}"
                            BorderThickness="1,0,0,0"
                            Opacity="0.8"
                            Visibility="{Binding ElementName=LeftScrollButtonElement,
                                                 Path=Visibility}" />
                    <ScrollViewer x:Name="ScrollViewerElement"
                                  HorizontalScrollBarVisibility="{Binding (ScrollViewer.HorizontalScrollBarVisibility),
                                                                          RelativeSource={RelativeSource TemplatedParent}}"
                                  IsTabStop="False"
                                  Style="{StaticResource EmptyScrollViewerStyle}"
                                  VerticalScrollBarVisibility="Disabled">
                        <ItemsPresenter />
                    </ScrollViewer>
                    <Border x:Name="PART_RightSeparator"
                            MinHeight="18"
                            Margin="3,0,0,3"
                            BorderBrush="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}"
                            BorderThickness="0,0,1,0"
                            Opacity="0.8" />
                    <RepeatButton x:Name="RightScrollButtonElement"
                                  MinWidth="26"
                                  MinHeight="26"
                                  Margin="5,0,0,0"
                                  Padding="2"
                                  helpers:ThemeHelper.DisabledBrush="Transparent"
                                  telerik:RadDockPanel.Dock="Right"
                                  AutomationProperties.Name="scroll right"
                                  Background="Transparent"
                                  BorderBrush="Transparent"
                                  Foreground="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}">
                        <telerik:RadGlyph FontSize="16"
                                          Glyph="{StaticResource GlyphArrowChevronRight}" />
                    </RepeatButton>
                    <telerik:RadButton x:Name="PART_AddItemsButton"
                                       Margin="5,0"
                                       helpers:ThemeHelper.FocusBrush="{x:Null}"
                                       Style="{TemplateBinding AddButtonStyle}"
                                       Visibility="{TemplateBinding AddButtonVisibility}">
                        <telerik:RadGlyph x:Name="PlusGlyph"
                                          FontSize="16"
                                          Glyph="{StaticResource GlyphPlus}" />
                    </telerik:RadButton>
                </panels:RadTabbedWindowPanel>
                <Canvas x:Name="DropIndicatorPanel"
                        Grid.Column="1"
                        HorizontalAlignment="Left"
                        Visibility="Collapsed">
                    <Border x:Name="DropIndicator"
                            VerticalAlignment="Center"
                            Background="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}"
                            BorderBrush="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}"
                            IsHitTestVisible="False"
                            Opacity="0.3" />
                </Canvas>
            </Grid>
            <Border x:Name="ContentBorder"
                    Grid.Row="1"
                    Padding="{TemplateBinding Padding}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent},
                                              Path=BorderThickness,
                                              Mode=TwoWay}"
                    CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent},
                                           Path=(helpers:ThemeHelper.CornerRadius),
                                           Converter={StaticResource CornerRadiusConverter},
                                           ConverterParameter=BottomLeftRight}">
                <Grid telerik:CornerRadiusHelper.ClipRadius="{Binding ElementName=ContentBorder,
                                                                      Path=CornerRadius}"
                      telerik:CornerRadiusHelper.ClipRadiusOffset="{TemplateBinding telerik:CornerRadiusHelper.ClipRadiusOffset}">
                    <ContentPresenter x:Name="ContentElement"
                                      telerik:CornerRadiusHelper.ClipRadius="{Binding ElementName=ContentBorder,
                                                                                      Path=CornerRadius}"
                                      telerik:CornerRadiusHelper.ClipRadiusOffset="{TemplateBinding telerik:CornerRadiusHelper.ClipRadiusOffset}"
                                      ContentTemplate="{TemplateBinding SelectedContentTemplate}"
                                      ContentTemplateSelector="{TemplateBinding SelectedContentTemplateSelector}"
                                      Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                           Path=IsContentPreserved,
                                                           Converter={StaticResource InvertedBooleanToVisibilityConverter}}" />
                    <Grid x:Name="ContentElementsPanel"
                          Visibility="{Binding RelativeSource={RelativeSource TemplatedParent},
                                               Path=IsContentPreserved,
                                               Converter={StaticResource BooleanToVisibilityConverter}}" />
                </Grid>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="TabStripPlacement" Value="Top">
                <Setter TargetName="ContentBorder" Property="BorderThickness" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}" />
            </Trigger>
            <Trigger SourceName="LeftScrollButtonElement" Property="IsMouseOver" Value="True">
                <Setter TargetName="LeftScrollButtonElement" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
            </Trigger>
            <Trigger SourceName="RightScrollButtonElement" Property="IsMouseOver" Value="True">
                <Setter TargetName="RightScrollButtonElement" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
            </Trigger>
            <Trigger SourceName="LeftScrollButtonElement" Property="IsPressed" Value="True">
                <Setter TargetName="LeftScrollButtonElement" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
            </Trigger>
            <Trigger SourceName="RightScrollButtonElement" Property="IsPressed" Value="True">
                <Setter TargetName="RightScrollButtonElement" Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
            </Trigger>
            <Trigger SourceName="LeftScrollButtonElement" Property="IsEnabled" Value="False">
                <Setter TargetName="LeftScrollButtonElement" Property="BorderThickness" Value="0" />
            </Trigger>
            <Trigger SourceName="RightScrollButtonElement" Property="IsEnabled" Value="False">
                <Setter TargetName="RightScrollButtonElement" Property="BorderThickness" Value="0" />
            </Trigger>
            <Trigger SourceName="HeaderBorder" Property="BorderThickness" Value="1">
                <Setter TargetName="PART_HeaderGrid" Property="Margin" Value="0" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="TabbedWindowTabControlStyle"
           BasedOn="{StaticResource RadTabControlStyle}"
           TargetType="tabbedWindow:TabbedWindowTabControl">
        <Setter Property="CloseTabsOnMouseMiddleButtonDown" Value="True" />
        <Setter Property="Template" Value="{StaticResource TabbedWindowTabControlTemplate}" />
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
        <Setter Property="AddButtonStyle" Value="{StaticResource AddItemsButtonStyle}" />
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="HeaderBackground" Value="{telerik1:Office2019Resource ResourceKey=HeaderBackgroundBrush}" />
        <Setter Property="ItemContainerStyle" Value="{DynamicResource RadTabbedWindowTabItemStyle}" />
        <Setter Property="telerik:CornerRadiusHelper.ClipRadiusOffset" Value="-1" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikNavigationPrimitives:RadBrowserPanel ItemMinWidth="{Binding ItemMinWidth,
                                                                                        FallbackValue=80,
                                                                                        RelativeSource={RelativeSource AncestorType={x:Type tabbedWindow:TabbedWindowTabControl}}}"
                                                                 ItemWidth="{Binding ItemWidth,
                                                                                     FallbackValue=120,
                                                                                     RelativeSource={RelativeSource AncestorType={x:Type tabbedWindow:TabbedWindowTabControl}}}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
