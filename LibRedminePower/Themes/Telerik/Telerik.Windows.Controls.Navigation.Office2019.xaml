    <Style x:Key="LayoutControlManipulationPaneStyle"
           TargetType="layoutControl:LayoutControlManipulationPane">
        <Setter Property="Background" Value="{telerik1:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="10" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlManipulationPane">
                    <Grid>
                        <Border Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <StackPanel Margin="{TemplateBinding Padding}"
                                            HorizontalAlignment="Center"
                                            Orientation="Horizontal">
                                    <telerik:RadRadioButton Content="{StaticResource GlyphAlignLeftElement}"
                                                            IsChecked="{Binding ContextItem.HorizontalAlignment,
                                                                                RelativeSource={RelativeSource TemplatedParent},
                                                                                Converter={StaticResource EnumToBooleanConverter},
                                                                                ConverterParameter=Left,
                                                                                Mode=TwoWay}">
                                        <telerik:RadRadioButton.Style>
                                            <Style BasedOn="{StaticResource ManipulationPaneRadioButtonStyle}"
                                                   TargetType="{x:Type telerik:RadRadioButton}">
                                                <Style.Triggers>
                                                    <Trigger Property="FlowDirection" Value="LeftToRight">
                                                        <Setter Property="telerik:RadToolTipService.ToolTipContent">
                                                            <Setter.Value>
                                                                <telerik:RadToolTipContentView>
                                                                    <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Left}" />
                                                                </telerik:RadToolTipContentView>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </Trigger>
                                                    <Trigger Property="FlowDirection" Value="RightToLeft">
                                                        <Setter Property="telerik:RadToolTipService.ToolTipContent">
                                                            <Setter.Value>
                                                                <telerik:RadToolTipContentView>
                                                                    <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Right}" />
                                                                </telerik:RadToolTipContentView>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </Trigger>
                                                </Style.Triggers>
                                            </Style>
                                        </telerik:RadRadioButton.Style>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton Content="{StaticResource GlyphAlignCenterElement}"
                                                            IsChecked="{Binding ContextItem.HorizontalAlignment,
                                                                                RelativeSource={RelativeSource TemplatedParent},
                                                                                Converter={StaticResource EnumToBooleanConverter},
                                                                                ConverterParameter=Center,
                                                                                Mode=TwoWay}"
                                                            Style="{StaticResource ManipulationPaneRadioButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Center}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton Content="{StaticResource GlyphAlignRightElement}"
                                                            IsChecked="{Binding ContextItem.HorizontalAlignment,
                                                                                RelativeSource={RelativeSource TemplatedParent},
                                                                                Converter={StaticResource EnumToBooleanConverter},
                                                                                ConverterParameter=Right,
                                                                                Mode=TwoWay}">
                                        <telerik:RadRadioButton.Style>
                                            <Style BasedOn="{StaticResource ManipulationPaneRadioButtonStyle}"
                                                   TargetType="{x:Type telerik:RadRadioButton}">
                                                <Style.Triggers>
                                                    <Trigger Property="FlowDirection" Value="LeftToRight">
                                                        <Setter Property="telerik:RadToolTipService.ToolTipContent">
                                                            <Setter.Value>
                                                                <telerik:RadToolTipContentView>
                                                                    <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Right}" />
                                                                </telerik:RadToolTipContentView>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </Trigger>
                                                    <Trigger Property="FlowDirection" Value="RightToLeft">
                                                        <Setter Property="telerik:RadToolTipService.ToolTipContent">
                                                            <Setter.Value>
                                                                <telerik:RadToolTipContentView>
                                                                    <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Left}" />
                                                                </telerik:RadToolTipContentView>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </Trigger>
                                                </Style.Triggers>
                                            </Style>
                                        </telerik:RadRadioButton.Style>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton Margin="0"
                                                            Content="{StaticResource GlyphArrowsResizing}"
                                                            IsChecked="{Binding ContextItem.HorizontalAlignment,
                                                                                RelativeSource={RelativeSource TemplatedParent},
                                                                                Converter={StaticResource EnumToBooleanConverter},
                                                                                ConverterParameter=Stretch,
                                                                                Mode=TwoWay}"
                                                            Style="{StaticResource ManipulationPaneRadioButtonStyle}">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Stretch}" />
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                </StackPanel>
                                <StackPanel Grid.Row="1"
                                            Margin="{Binding Padding,
                                                             RelativeSource={RelativeSource TemplatedParent},
                                                             Converter={StaticResource ThicknessToOrientedThicknessConverter},
                                                             ConverterParameter=LeftRightBottom}"
                                            VerticalAlignment="Center"
                                            Orientation="Horizontal">
                                    <telerik:RadRadioButton Content="{StaticResource GlyphAlignTopElement}"
