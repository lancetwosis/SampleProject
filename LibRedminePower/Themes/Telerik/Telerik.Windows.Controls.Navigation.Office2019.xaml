                        <Path x:Name="PART_ArrowGlyph"
                              Width="14"
                              Height="10"
                              Margin="0,4,0,0"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Top"
                              Canvas.ZIndex="102"
                              Data="{telerik:RadGlyph Glyph={StaticResource GlyphArrowChevronUp},
                                                      Font=TelerikWebUI}"
                              Fill="{TemplateBinding Foreground}"
                              IsHitTestVisible="False"
                              Stretch="Uniform" />
                        <Path x:Name="PART_PathElement"
                              Canvas.ZIndex="100"
                              Fill="{TemplateBinding Background}"
                              Stroke="{TemplateBinding BorderBrush}"
                              StrokeThickness="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                        Path=BorderThickness.Left}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="PART_PathElement" Property="Fill" Value="{telerik:Office2019Resource ResourceKey=AccentMouseOverBackgroundBrush}" />
                            <Setter TargetName="PART_ArrowGlyph" Property="Fill" Value="{telerik:Office2019Resource ResourceKey=HighlightedForegroundBrush}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="PART_PathElement" Property="Fill" Value="{x:Null}" />
                            <Setter TargetName="PART_ArrowGlyph" Property="Fill" Value="{telerik:Office2019Resource ResourceKey=DisabledCheckedForegroundBrush}" />
                            <Setter TargetName="PART_ArrowGlyph" Property="Opacity" Value="{telerik1:Office2019Resource ResourceKey=DisabledOpacity}" />
