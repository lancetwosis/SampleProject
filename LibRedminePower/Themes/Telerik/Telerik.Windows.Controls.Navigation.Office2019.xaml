    <Style BasedOn="{StaticResource TabbedWindowTabControlStyle}"
           TargetType="tabbedWindow:TabbedWindowTabControl" />
    <Style x:Key="TabbedWindowHeaderButtonStyle"
           BasedOn="{StaticResource WindowButtonStyle}"
           TargetType="telerik:RadButton">
        <Setter Property="MinWidth" Value="26" />
        <Setter Property="MinHeight" Value="26" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}" />
    </Style>
    <ControlTemplate x:Key="RadTabbedWindowTemplate"
                     TargetType="telerikNavigation:RadTabbedWindow">
        <Grid x:Name="LayoutRoot">
            <Border x:Name="WindowOuterBorder"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{TemplateBinding CornerRadius}">
                <AdornerDecorator>
                    <Border x:Name="Header"
                            Margin="{TemplateBinding Padding}"
                            Background="{TemplateBinding BorderBackground}"
                            CornerRadius="{Binding CornerRadius,
                                                   ElementName=WindowOuterBorder}">
                        <tabbedWindow:TabbedWindowTabControl x:Name="PART_TabControl"
                                                             helpers:ThemeHelper.CornerRadius="{TemplateBinding CornerRadius}"
                                                             AddButtonStyle="{TemplateBinding AddButtonStyle}"
                                                             AddButtonVisibility="{TemplateBinding AddButtonVisibility}"
                                                             ContentTemplate="{TemplateBinding ContentTemplate}"
                                                             ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                                             DisplayMemberPath="{TemplateBinding DisplayMemberPath}"
                                                             IsContentPreserved="{TemplateBinding IsContentPreserved}"
                                                             ItemContainerStyle="{TemplateBinding ItemContainerStyle}"
                                                             ItemContainerStyleSelector="{TemplateBinding ItemContainerStyleSelector}"
                                                             ItemMinWidth="{TemplateBinding ItemMinWidth}"
                                                             ItemTemplate="{TemplateBinding ItemTemplate}"
                                                             ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}"
                                                             ItemWidth="{TemplateBinding ItemWidth}"
                                                             ItemsPanel="{TemplateBinding ItemsPanel}"
                                                             ItemsSource="{TemplateBinding ItemsSource}"
                                                             ScrollMode="{TemplateBinding ScrollMode}"
                                                             SelectedIndex="{Binding SelectedIndex,
                                                                                     RelativeSource={RelativeSource TemplatedParent},
                                                                                     Mode=TwoWay}"
                                                             SelectedItem="{Binding SelectedItem,
                                                                                    RelativeSource={RelativeSource TemplatedParent},
                                                                                    Mode=TwoWay}"
                                                             SelectedItemRemoveBehaviour="{TemplateBinding SelectedItemRemoveBehaviour}">
                            <tabbedWindow:TabbedWindowTabControl.AdditionalLeftContent>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <ContentPresenter x:Name="IconContent"
                                                      Grid.Column="0"
                                                      Margin="5,0,0,0"
                                                      HorizontalAlignment="Left"
                                                      VerticalAlignment="Center"
                                                      Content="{TemplateBinding Icon}"
                                                      ContentTemplate="{TemplateBinding IconTemplate}" />
                                    <ContentPresenter x:Name="HeaderContent"
                                                      Grid.Column="1"
                                                      Margin="5,0"
                                                      HorizontalAlignment="Stretch"
                                                      VerticalAlignment="Center"
                                                      Content="{TemplateBinding Header}"
                                                      ContentTemplate="{TemplateBinding HeaderTemplate}"
                                                      ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                                      SnapsToDevicePixels="True"
                                                      TextBlock.FontFamily="{TemplateBinding FontFamily}"
                                                      TextBlock.FontSize="{TemplateBinding FontSize}"
                                                      TextBlock.Foreground="{telerik1:Office2019Resource ResourceKey=HeaderForegroundBrush}" />
                                </Grid>
                            </tabbedWindow:TabbedWindowTabControl.AdditionalLeftContent>
                            <tabbedWindow:TabbedWindowTabControl.AdditionalContent>
                                <Grid x:Name="PART_HeaderButtonsBorder"
                                      VerticalAlignment="Top">
                                    <StackPanel x:Name="HeaderButtons"
                                                Orientation="Horizontal">
                                        <telerik:RadButton x:Name="PART_MinimizeButton"
                                                           Command="{x:Static telerik:WindowCommands.Minimize}"
                                                           Style="{StaticResource TabbedWindowHeaderButtonStyle}"
                                                           Visibility="{Binding IsEnabled,
                                                                                RelativeSource={RelativeSource Self},
                                                                                Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Minimize" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph HorizontalAlignment="Center"
                                                              VerticalAlignment="Center"
                                                              FontSize="16"
                                                              Glyph="{StaticResource GlyphMinimize}" />
                                        </telerik:RadButton>
                                        <telerik:RadButton x:Name="PART_RestoreButton"
                                                           Command="{x:Static telerik:WindowCommands.Restore}"
                                                           Style="{StaticResource TabbedWindowHeaderButtonStyle}"
                                                           Visibility="{Binding IsEnabled,
                                                                                RelativeSource={RelativeSource Self},
                                                                                Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Restore" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph HorizontalAlignment="Center"
                                                              VerticalAlignment="Center"
                                                              FontSize="16"
                                                              Glyph="{StaticResource GlyphWindowCollapse}" />
                                        </telerik:RadButton>
                                        <telerik:RadButton x:Name="PART_MaximizeButton"
                                                           Command="{x:Static telerik:WindowCommands.Maximize}"
                                                           Style="{StaticResource TabbedWindowHeaderButtonStyle}"
                                                           Visibility="{Binding IsEnabled,
                                                                                RelativeSource={RelativeSource Self},
                                                                                Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Maximize" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph HorizontalAlignment="Center"
                                                              VerticalAlignment="Center"
                                                              FontSize="16"
                                                              Glyph="{StaticResource GlyphWindow}" />
                                        </telerik:RadButton>
                                        <telerik:RadButton x:Name="PART_CloseButton"
                                                           Command="{x:Static telerik:WindowCommands.Close}"
                                                           Style="{StaticResource TabbedWindowHeaderButtonStyle}"
                                                           Visibility="{Binding IsEnabled,
                                                                                RelativeSource={RelativeSource Self},
                                                                                Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip telerik:LocalizationManager.ResourceKey="Close" />
                                            </ToolTipService.ToolTip>
                                            <telerik:RadGlyph HorizontalAlignment="Center"
                                                              VerticalAlignment="Center"
                                                              FontSize="16"
                                                              Glyph="{StaticResource GlyphClose}" />
                                        </telerik:RadButton>
                                    </StackPanel>
                                </Grid>
                            </tabbedWindow:TabbedWindowTabControl.AdditionalContent>
                        </tabbedWindow:TabbedWindowTabControl>
                    </Border>
                </AdornerDecorator>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="WindowState" Value="Maximized">
                <Setter TargetName="WindowOuterBorder" Property="BorderThickness" Value="0" />
                <Setter TargetName="LayoutRoot" Property="Margin" Value="0" />
                <Setter TargetName="Header" Property="Margin" Value="0" />
                <Setter TargetName="WindowOuterBorder" Property="CornerRadius" Value="0" />
                <Setter TargetName="PART_TabControl" Property="mat:MaterialAssist.CornerRadius" Value="0" />
            </Trigger>
            <Trigger Property="WindowState" Value="Minimized">
                <Setter TargetName="LayoutRoot" Property="HorizontalAlignment" Value="Left" />
                <Setter TargetName="LayoutRoot" Property="VerticalAlignment" Value="Top" />
                <Setter TargetName="LayoutRoot" Property="Margin" Value="0" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="HeaderContent" Property="Content" Value="{x:Null}" />
                    <Condition SourceName="HeaderContent" Property="ContentTemplate" Value="{x:Null}" />
                    <Condition SourceName="HeaderContent" Property="ContentTemplateSelector" Value="{x:Null}" />
                </MultiTrigger.Conditions>
                <Setter TargetName="HeaderContent" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="IconContent" Property="Content" Value="{x:Null}" />
                    <Condition SourceName="IconContent" Property="ContentTemplate" Value="{x:Null}" />
                </MultiTrigger.Conditions>
                <Setter TargetName="IconContent" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <Trigger Property="mat:MaterialAssist.IsShadowDisabled" Value="True">
                <Setter TargetName="LayoutRoot" Property="Margin" Value="0" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadTabbedWindowStyle"
           BasedOn="{StaticResource RadWindowStyle}"
           TargetType="telerikNavigation:RadTabbedWindow">
        <Setter Property="CornerRadius" Value="0" />
        <Setter Property="Template" Value="{StaticResource RadTabbedWindowTemplate}" />
        <Setter Property="AddButtonStyle" Value="{StaticResource AddItemsButtonStyle}" />
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Header" Value="{x:Null}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="MinHeight" Value="32" />
        <Setter Property="MinWidth" Value="150" />
        <Setter Property="ItemContainerStyle" Value="{StaticResource RadTabbedWindowTabItemStyle}" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikNavigationPrimitives:RadBrowserPanel ItemMinWidth="{Binding ItemMinWidth,
                                                                                        FallbackValue=80,
                                                                                        RelativeSource={RelativeSource AncestorType={x:Type telerikNavigation:RadTabbedWindow}}}"
                                                                 ItemWidth="{Binding ItemWidth,
                                                                                     FallbackValue=120,
                                                                                     RelativeSource={RelativeSource AncestorType={x:Type telerikNavigation:RadTabbedWindow}}}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="navigation:RadWindowInteropHelper.ShowInTaskbar" Value="True" />
        <Style.Triggers>
            <Trigger Property="navigation:RadWindowInteropHelper.AllowTransparency" Value="False">
                <Setter Property="navigation:RadWindowInteropHelper.ClipMaskCornerRadius" Value="0" />
                <Setter Property="animation:AnimationManager.IsAnimationEnabled" Value="False" />
                <Setter Property="mat:MaterialAssist.IsShadowDisabled" Value="True" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style BasedOn="{StaticResource RadTabbedWindowStyle}"
           TargetType="telerikNavigation:RadTabbedWindow" />
    <ControlTemplate x:Key="RadSplashScreenTemplate"
                     TargetType="telerikNavigation:RadSplashScreen">
        <Grid x:Name="LayoutRootGrid">
            <!--  RedminePowerカスタマイズ箇所↓↓  -->
            <!--  対応内容：#224 スプラッシュスクリーンの修正  影を付ける  -->
            <Border x:Name="ShadowBorder"
                    Margin="7"
                    Background="Transparent"
                    BorderThickness="0"
                    ClipToBounds="False">
                <Border.Effect>
                    <DropShadowEffect BlurRadius="7"
                                      Opacity="0.8"
                                      ShadowDepth="0"
                                      Color="Black" />
                </Border.Effect>
                <Border x:Name="BackgroundVisual"
                        Background="{TemplateBinding Background}"
                        BorderBrush="Black"
                        BorderThickness="0.5">
                    <Grid x:Name="ContentGrid"
                          Margin="{TemplateBinding Padding}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="Auto" />
                            <!--<RowDefinition Height="Auto" />-->
                            <!--<RowDefinition Height="Auto" />-->
                        </Grid.RowDefinitions>
                        <Image Width="{TemplateBinding ImageWidth}"
                               Height="{TemplateBinding ImageHeight}"
                               Source="{Binding Path=ImagePath,
                                                RelativeSource={RelativeSource TemplatedParent},
                                                TargetNullValue={x:Null}}"
                               Stretch="{TemplateBinding ImageStretch}"
                               Visibility="{Binding Path=ImagePath,
                                                    RelativeSource={RelativeSource TemplatedParent},
                                                    Converter={StaticResource NullToVisibilityConverter}}" />
                        <!--<ContentPresenter Grid.Row="1"
                                  Margin="0,20,0,0"
                                  HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                  ContentSource="Content"
                                  TextBlock.Foreground="{TemplateBinding Foreground}" />-->
                        <!--  対応内容：#224 スプラッシュスクリーンの修正  StackPanelの追加  -->
                        <StackPanel Grid.Row="1"
                                    Margin="0,20,0,0"
                                    Orientation="Horizontal">
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalFooterAlignment}"
                                              ContentSource="Footer"
                                              TextBlock.Foreground="{TemplateBinding Foreground}" />
                            <telerik1:RadProgressBar x:Name="LoadingProgressBar"
                                                     MaxWidth="200"
                                                     Foreground="Gray"
                                                     IsIndeterminate="{TemplateBinding IsIndeterminate}"
                                                     Maximum="{TemplateBinding MaxValue}"
                                                     Minimum="{TemplateBinding MinValue}"
                                                     Visibility="{TemplateBinding ProgressBarVisibility}"
                                                     Value="{TemplateBinding ProgressValue}" />
                        </StackPanel>
                        <!--  RedminePowerカスタマイズ箇所↑↑  -->
                    </Grid>
                </Border>
            </Border>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadSplashScreenStyle"
           TargetType="telerikNavigation:RadSplashScreen">
        <!--  RedminePowerカスタマイズ箇所↓↓  -->
        <!--  対応内容：#224 スプラッシュスクリーンの修正  -->
        <Setter Property="MinWidth" Value="500" />
        <Setter Property="MinHeight" Value="300" />
        <!--  RedminePowerカスタマイズ箇所↑↑  -->
        <Setter Property="FontFamily" Value="{telerik1:Office2019Resource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:Office2019Resource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik:Office2019Resource ResourceKey=MainForegroundBrush}" />
        <Setter Property="Background" Value="{telerik:Office2019Resource ResourceKey=MainBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{telerik:Office2019Resource ResourceKey=MainBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="20,20,20,10" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="Template" Value="{StaticResource RadSplashScreenTemplate}" />
    </Style>
    <Style BasedOn="{StaticResource RadSplashScreenStyle}"
           TargetType="telerikNavigation:RadSplashScreen" />
</ResourceDictionary>
