<UserControl x:Class="RedmineTimePuncher.Views.TableEditorView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:RedmineTimePuncher.Views"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:tableEditor="clr-namespace:RedmineTableEditor.Views;assembly=RedmineTableEditor"
             xmlns:vm="clr-namespace:RedmineTimePuncher.ViewModels.TableEditor"
             d:DataContext="{d:DesignInstance {x:Type vm:TableEditorViewModel},
                                              IsDesignTimeCreatable=True}"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <tableEditor:TableEditorView DataContext="{Binding ViewModel}" />
</UserControl>
